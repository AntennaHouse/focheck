# @axf:footnote-stacking
# 
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf.footnote-stacking
axf_footnote-stacking =
  
  ## 脚注の配置の方法
  attribute axf:footnote-stacking { "block" | "inline" }?
# @axf:footnote-position
# 
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf.footnote-position
axf_footnote-position =
  
  ## 脚注を配置する場所
  attribute axf:footnote-position {
    "page"
    | "start"
    | "end"
    | "inside"
    | "outside"
    | "column"
    | "start-column"
    | "end-column"
    | "inside-column"
    | "outside-column"
  }?
# @axf:footnote-keep
# 
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf.footnote-keep
axf_footnote-keep =
  
  ## 脚注とアンカを同一ページ内または段内に配置するように
  attribute axf:footnote-keep { "auto" | "always" }?
# @axf:footnote-max-height
# 
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf.footnote-max-height
axf_footnote-max-height =
  
  ## 脚注の最大高さ： auto | <length> | <percentage>
  attribute axf:footnote-max-height { text }?
# @axf:outline-expand
# 
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf.outline-expand
axf_outline-expand =
  
  ## しおりの初期状態で、下位の階層を表示するかどうか
  attribute axf:outline-expand { "true" | "false" }?
# @axf:outline-group
# 
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf.outline-group
axf_outline-group =
  
  ## 出力グループの名： <string>
  attribute axf:outline-group { text }?
# @axf:outline-level
# 
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf.outline-level
axf_outline-level =
  
  ## しおりの項目の階層レベル
  attribute axf:outline-level { text }?
# @axf:outline-title
# 
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf.outline-title
axf_outline-title =
  
  ## しおりのタイトルとして表示される文字列
  attribute axf:outline-title { text }?
# @axf:outline-color
# 
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf.outline-color
axf_outline-color =
  
  ## しおりのタイトルとして表示される文字列の色
  attribute axf:outline-color { text }?
# @axf:outline-font-style
# 
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf_outline-font-style
axf_outline-font-style =
  
  ## しおりのタイトルとして表示される文字列のフォントスタイル
  attribute axf:outline-font-style { "normal" | "italic" }?
# @axf:outline-font-weight
# 
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf:outline-font-weight
axf_outline-font-weight =
  
  ## しおりのタイトルとして表示される文字列のフォントの太さ
  attribute axf:outline-font-weight { "normal" | "bold" }?
# @axf:printer-marks
# 
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf.printer-marks
axf_printer-marks =
  
