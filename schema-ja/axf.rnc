namespace a = "http://relaxng.org/ns/compatibility/annotations/1.0"
namespace axf = "http://www.antennahouse.com/names/XSL/Extensions"
namespace axh = "http://www.antennahouse.com/names/XSL/Hyphenations"
namespace axs = "http://www.antennahouse.com/names/XSL/Settings"
namespace fo = "http://www.w3.org/1999/XSL/Format"
namespace local = ""
namespace m = "http://www.w3.org/1998/Math/MathML"
namespace svg = "http://www.w3.org/2000/svg"


## ハイフネーション 指定
include "axh.rnc" inherit = axf

## オプション指定
include "axs.rnc" inherit = axs
include "axf-inherited.rnc" inherit = axf {
  # 
  # Overrides to patterns in 'axf-inherited.rnc' (and patterns in
  # modules that it includes).
  #
  
  # https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#fo.change-bar-begin
  fo_change-bar-begin.model = fo_float?
  # Nested fo:page-sequence
  # https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#fo.page-sequence
  fo_flow.model =
    fo_marker*,
    ((block.fo.list | neutral.fo.list.block | fo_page-sequence)+
     & point.fo.list*)
  fo_layout-master-set.model =
    (axf_spread-page-master
     | fo_simple-page-master
     | fo_page-sequence-master
     | fo_flow-map)+
  # https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.document-info
  fo_root.model =
    axf_document-info*,
    fo_layout-master-set,
    axf_document-info*,
    fo_declarations?,
    axf_document-info*,
    fo_bookmark-tree?,
    axf_document-info*,
    (fo_page-sequence | fo_page-sequence-wrapper)+
  # AH Formatter allows fo:region-* in any order.
  fo_simple-page-master.model =
    fo_region-body+
    & fo_region-before?
    & fo_region-after?
    & fo_region-start?
    & fo_region-end?
  # AH Formatter uses some axf:* elements within fo:declarations.
  fo_declarations.model =
    (fo_color-profile
     | axf_counter-style
     | axf_custom-property
     | axf_document-info
     | axf_font-face
     | axf_formatter-config
     | axf_hyphenation-info
     | axf_space-between-digit-and-punctuation
     | axf_space-between-punctuation-and-digit
     | axf_space-end-punctuation
     | axf_space-start-punctuation
     | axf_output-volume-info
     | non-xsl-element)*
  # AH Formatter allows axf:document-info with multi-volume output.
  fo_page-sequence.model =
    axf_document-info?,
    fo_title?,
    fo_folio-prefix?,
    fo_folio-suffix?,
    fo_static-content*,
    fo_flow+
  # AH Formatter allows empty fo:static-content.
  fo_static-content.model =
    (block.fo.list | neutral.fo.list)* & point.fo.list*
  # AH Formatter allows fo:block-container to omit block FOs.
  fo_block-container.model =
    fo_marker*,
    ((block.fo.list | neutral.fo.list)* & point.fo.list*)
  # AH Formatter allows fo:inline-container to omit block FOs.
  fo_inline-container.model =
    fo_marker*,
    ((block.fo.list | neutral.fo.list)* & point.fo.list*)
  # AH Formatter allows empty fo:list-item.
  fo_list-item.model =
    fo_marker*,
    ((fo_list-item-label, fo_list-item-body)? & point.fo.list*)
  # AH Formatter allows fo:list-item-label omit block FOs.
  fo_list-item-label.model =
    fo_marker*,
    ((block.fo.list | neutral.fo.list)* & point.fo.list*)
  # AH Formatter allows fo:table-cell to omit block FOs.
  # axf:table-cell-repeated-marker used when cell repeats.
  fo_table-cell.model =
    fo_marker*,
    axf_table-cell-repeated-marker?,
    ((block.fo.list | neutral.fo.list)* & point.fo.list*)
  # For fo:instream-foreign-object
  # Exclude axf:* elements and attributes from counting as 'non-xsl'.
  non-xsl =
    attribute * - (local:* | xml:* | axf:*) { text }*,
    element * - (local:* | fo:* | axf:* | m:* | svg:*) {
      attribute * { text }*,
      anything
    }*
  # For fo:declarations
  # Exclude axf:* elements from counting as 'non-xsl'.
  non-xsl-element =
    element * - (local:* | fo:* | axf:* | m:* | svg:*) {
      attribute * { text }*,
      anything
    }*
  # 
  # End of overrides to patterns in 'axf-inherited.rnc'.
  #
  
}
# 
# Antenna House extension elements
#

# <axf:counter-style/>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.counter-style
axf_counter-style =
  
  ## PDF 1.4 以降のとき Custom PDF は有効です。PDF/A では無効です。
  element axf:counter-style {
    
    ## Name of the counter style: <name>.
    attribute name { text },
    
    ## Type of the counter system: cyclic | numeric | alphabetic | symbolic | additive | [fixed <integer>?] | [ extends <counter-style-name> ]
    attribute system { text }?,
    
    ## Prefix and suffix for a negative value: <symbol> <symbol>?
    attribute negative { text }?,
    
    ## Prefix: <symbol>
    attribute prefix { text }?,
    
    ## Suffix: <symbol>
    attribute suffix { text }?,
    
    ## Range to which the counter style is applied: [ [ <integer> | infinite ]{2} ]# | auto
    attribute range { text }?,
    
    ## Padding for a small digit: <integer> && <symbol>
    attribute pad { text }?,
    
    ## Fallback style: <counter-style-name>
    attribute fallback { text }?,
    
    ## Counter symbols: <symbol>+
    attribute symbols { text }?,
    
    ## Weighted counter symbols: [ <integer> && <symbol> ]#
    attribute additive-symbols { text }?,
    empty
  }
# <axf:custom-property/>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.custom-property
axf_custom-property =
  
  ## PDF 1.4 以降のとき Custom PDF は有効です。PDF/A では無効です。
  element axf:custom-property {
    
    ## カスタムプロパティ名どちらも空ではいけません。
    attribute name { text },
    
    ## カスタムプロパティの値どちらも空ではいけません。
    attribute value { text },
    empty
  }
# <axf:document-info/>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.document-info
axf_document-info =
  
  ## PDF には、著者やタイトルなどの文書情報を埋め込んでおくことができます 
  element axf:document-info {
    # 'name="title"' was used in V5.1 and still works but is
    # undocumented in V6.2+.
    
    ## 文字情報の名
    attribute name {
      "document-title"
      | "title"
      | "subject"
      | "author"
      | "author-title"
      | "description-writer"
      | "keywords"
      | "copyright-status"
      | "copyright-notice"
      | "copyright-info-url"
      | "xmp"
      | "pagemode"
      | "pagelayout"
      | "hidetoolbar"
      | "hidemenubar"
      | "hidewindowui"
      | "fitwindow"
      | "centerwindow"
      | "displaydoctitle"
      | "openaction"
    },
    
    ## 文字情報の値ある情は許容値の制約があります。
    attribute value { text },
    empty
  }
# <axf:font-face>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.font-face
axf_font-face =
  
  ## 追加のハイフネーション情報
  element axf:font-face { axf_font-face.attlist, axf_font-face.model }
axf_font-face.model = empty
axf_font-face.attlist =
  attribute src { text },
  attribute font-family { text },
  attribute font-style { "normal" | "italic" }?,
  attribute font-weight {
    "normal"
    | "bold"
    | "100"
    | "200"
    | "300"
    | "400"
    | "500"
    | "600"
    | "700"
    | "800"
    | "900"
  }?
# <axf:footnote-number/>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.footnote-number
axf_footnote-number =
  
  ## 脚注番号を生成します
  element axf:footnote-number { id, empty }
# <axf:footnote-number-citation/>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.footnote-number-citation
axf_footnote-number-citation =
  
  ## 脚注番号を引用します
  element axf:footnote-number-citation {
    attribute ref-id { text },
    empty
  }
# <axf:form>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.form
axf_form = element axf:form { axf_form.attlist, axf_form.model }
axf_form.attlist =
  axf_field-submit-coordinates?,
  axf_field-submit-method?,
  external-destination?
axf_form.model = block.fo.list*
# <axf:form-field>
# 
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.form-field
axf_form-field =
  
  ## フォームフィールドを生成します。
  element axf:form-field {
    axf_form-field.attlist, axf_form-field.model
  }
axf_form-field.model =
  (axf_form-field-option | axf_form-field-event | fo_external-graphic)*
axf_form-field.attlist =
  # 'action-type' is required for <axf:form-field-event>.
  axf_action-type.form,
  axf_field-button-face,
  axf_field-button-face-down,
  axf_field-button-face-rollover,
  axf_field-button-icon,
  axf_field-button-icon-down,
  axf_field-button-icon-rollover,
  axf_field-button-layout,
  axf_field-checked,
  axf_field-checked-style,
  axf_field-default-text,
  axf_field-description,
  axf_field-editable,
  axf_field-flags,
  axf_field-font-size,
  axf_field-format,
  axf_field-format-category,
  axf_field-maxlen,
  axf_field-multiline,
  axf_field-multiple,
  axf_field-name,
  axf_field-name-suffix-page-number,
  axf_field-scroll,
  axf_field-text-align,
  axf_field-top-index,
  axf_field-type,
  axf_field-value,
  common-border-padding-and-background-properties,
  external-destination,
  height,
  internal-destination,
  width
# <axf:form-field-event>
# 
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.form-field-event
axf_form-field-event =
  
  ## イベントに対応したフォームアクションを定義します。
  element axf:form-field-event { axf_form-field-event.attlist, text }
axf_form-field-event.attlist =
  
  ## イベントの種類： MouseUp | MouseDown | MouseEnter | MouseExit | OnFocus | OnBlur
  attribute axf:name | name {
    "MouseUp"
    | "MouseDown"
    | "MouseEnter"
    | "MouseExit"
    | "OnFocus"
    | "OnBlur"
  },
  
  ## フォームアクション： auto | gotor | gotor | launch | uri | reset | submit | javascript
  attribute axf:action-type | action-type {
    axf_action-type.form.model | "javascript"
  }
# <axf:form-field-option>
# 
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.form-field-option
axf_form-field-option =
  
  ## リストボックス、コンボボックスの構成要素を指定します。 
  element axf:form-field-option { axf_form-field-option.attlist, text }
axf_form-field-option.attlist = axf_field-value
# <axf:formatter-config>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.formatter-config
axf_formatter-config =
  
  ## 追加のハイフネーション情報
  element axf:formatter-config {
    axf_formatter-config.attlist, formatter-config.model
  }
# axf_formatter-config.model =
#   axs_pdf-settings?
axf_formatter-config.attlist =
  
  ## 外部にあるオプション設定ファイル
  attribute src { text }?
# <axf:hyphenation-info>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.hyphenation-info
axf_hyphenation-info =
  
  ## 追加のハイフネーション情報
  element axf:hyphenation-info {
    axf_hyphenation-info.attlist, axh_exceptions?
  }
axf_hyphenation-info.attlist =
  attribute language { text }?,
  attribute src { text }?
# <axf:output-volume-info>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.output-volume-info
axf_output-volume-info =
  
  ## 多分冊出力指定
  element axf:output-volume-info {
    axf_output-volume-info.attlist, empty
  }
axf_output-volume-info.attlist =
  
  ## 初期文書番号： <positive-integer>
  attribute initial-volume-number { xsd:positiveInteger }?,
  
  ## 書式指定： <string>
  attribute format { text }?,
  
  ## 多分冊にしおりを付ける方法: first | all | separate | separate-group
  attribute bookmark-include {
    "first" | "all" | "separate" | "separate-group"
  }?,
  
  ## How to include document information in multiple separate volumes: first | all
  attribute document-info-include { "first" | "all" }?
# <axf:ruby>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ruby.html#axf.ruby
axf_ruby =
  
  ## ルビ構造
  element axf:ruby { axf_ruby.attlist, (axf_ruby-base, axf_ruby-text)+ }
axf_ruby.attlist =
  axf_ruby-align,
  axf_ruby-position,
  axf_ruby-offset,
  axf_ruby-overhang,
  axf_ruby-limit-overhang,
  axf_ruby-limit-space,
  axf_ruby-small-kana,
  axf_ruby-font-family,
  axf_ruby-font-size,
  axf_ruby-minimum-font-size,
  axf_ruby-font-style,
  axf_ruby-font-weight,
  axf_ruby-font-stretch,
  axf_ruby-condense,
  axf_ruby-color,
  common-border-padding-and-background-properties
# <axf:ruby-base>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ruby.html#axf.ruby-base
axf_ruby-base =
  
  ## ルビの親文字
  element axf:ruby-base { axf_ruby-base.attlist, (text | axf_ruby) }
axf_ruby-base.attlist = common-border-padding-and-background-properties
# <axf:ruby-text>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ruby.html#axf.ruby-text
axf_ruby-text =
  
  ## ルビ
  element axf:ruby-text { text }
# <axf:space-end-punctuation>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ruby.html#axf.space-end-punctuation
axf_space-end-punctuation =
  
  ## 約物の後の空白量
  element axf:space-end-punctuation { axf.european-typography.attlist }
# <axf:space-start-punctuation>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ruby.html#axf.space-start-punctuation
axf_space-start-punctuation =
  
  ## 約物の前の空白量
  element axf:space-start-punctuation {
    axf.european-typography.attlist
  }
# <axf:space-between-digit-and-punctuation>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ruby.html#axf.space-between-digit-and-punctuation
axf_space-between-digit-and-punctuation =
  
  ## 約物の前の空白量
  element axf:space-between-digit-and-punctuation {
    axf.european-typography.attlist
  }
# <axf:space-between-punctuation-and-digit>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ruby.html#axf.space-between-punctuation-and-digit
axf_space-between-punctuation-and-digit =
  
  ## 約物の前の空白量
  element axf:space-between-punctuation-and-digit {
    axf.european-typography.attlist
  }
axf.european-typography.attlist =
  
  ## 言語コード
  attribute language { text },
  
  ## 空白量： <number> | <length> | <percentage> | figure | punctuation | thin | hair
  attribute space { text }?,
  
  ## コードポイント
  attribute code { text }
# <axf:spread-page-master>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-spread.html#axf.spread-page-master
axf_spread-page-master =
  
  ## 見開きページマスタ
  element axf:spread-page-master {
    axf_background-color,
    axf_background-clip,
    axf_background-content-height,
    axf_background-content-type,
    axf_background-content-width,
    axf_background-image,
    axf_background-origin,
    axf_background-position-horizontal,
    axf_background-position-vertical,
    axf_background-repeat,
    axf_background-scaling,
    axf_background-size,
    axf_bleed,
    axf_left-page-master-reference,
    axf_right-page-master-reference,
    background-attachment,
    background-image,
    background-position,
    background-position-horizontal,
    background-position-vertical,
    background-repeat,
    master-name,
    writing-mode,
    axf_spread-region*
  }
# <axf:spread-region>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-spread.html#axf.spread-region
axf_spread-region =
  
  ## 見開きページの領域
  element axf:spread-region {
    common-absolute-position-properties,
    common-border-padding-and-background-properties,
    common-margin-properties-block,
    height,
    region-name,
    reference-orientation,
    overflow,
    width,
    writing-mode,
    empty
  }
# <axf:tab>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.tab
axf_tab =
  
  ## タブ位置までスペースを空けます
  element axf:tab { axf_tab-align, empty }
# <axf:table-cell-repeated-marker>
# https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.table-cell-repeated-marker
axf_table-cell-repeated-marker =
  
  ## 内容が axf:repeat-cell-content-at-break="true" の場合に使われます
  element axf:table-cell-repeated-marker {
    (text | inline.fo.list | block.fo.list)*
  }
