namespace a = "http://relaxng.org/ns/compatibility/annotations/1.0"
namespace axf = "http://www.antennahouse.com/names/XSL/Extensions"
namespace axh = "http://www.antennahouse.com/names/XSL/Hyphenations"
namespace fo = "http://www.w3.org/1999/XSL/Format"
namespace local = ""

include "axh.rnc" inherit = axf
include "axf-inherited.rnc" inherit = axf {
  # 
  # Overrides to patterns in 'axf-inherited.rnc' (and patterns in
  # modules that it includes).
  #
  
  # https://www.antennahouse.com/product/ahf65/ahf-ext.html#fo.change-bar-begin
  fo_change-bar-begin.model = fo_float?
  # Nested fo:page-sequence
  # https://www.antennahouse.com/product/ahf65/ahf-ext.html#fo.page-sequence
  fo_flow.model =
    fo_marker*,
    ((block.fo.list | neutral.fo.list.block | fo_page-sequence)+
     & point.fo.list*)
  fo_layout-master-set.model =
    (axf_spread-page-master
     | fo_simple-page-master
     | fo_page-sequence-master
     | fo_flow-map)+
  # https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.document-info
  fo_root.model =
    axf_document-info*,
    fo_layout-master-set,
    axf_document-info*,
    fo_declarations?,
    axf_document-info*,
    fo_bookmark-tree?,
    axf_document-info*,
    (fo_page-sequence | fo_page-sequence-wrapper)+
  # AH Formatter allows fo:region-* in any order.
  fo_simple-page-master.model =
    fo_region-body+
    & fo_region-before?
    & fo_region-after?
    & fo_region-start?
    & fo_region-end?
  # AH Formatter uses axf:custom-property and axf:document-info within
  # fo:declarations.
  fo_declarations.model |=
    (axf_custom-property | axf_document-info | axf_hyphenation-info)*
  # AH Formatter allows empty fo:static-content.
  fo_static-content.model =
    (block.fo.list | neutral.fo.list)* & point.fo.list*
  # AH Formatter allows fo:block-container to omit block FOs.
  fo_block-container.model =
    fo_marker*,
    ((block.fo.list | neutral.fo.list)* & point.fo.list*)
  # AH Formatter allows fo:inline-container to omit block FOs.
  fo_inline-container.model =
    fo_marker*,
    ((block.fo.list | neutral.fo.list)* & point.fo.list*)
  # AH Formatter allows empty fo:list-item.
  fo_list-item.model =
    fo_marker*,
    ((fo_list-item-label, fo_list-item-body)? & point.fo.list*)
  # AH Formatter allows fo:list-item-label omit block FOs.
  fo_list-item-label.model =
    fo_marker*,
    ((block.fo.list | neutral.fo.list)* & point.fo.list*)
  # AH Formatter allows fo:table-cell to omit block FOs.
  # axf:table-cell-repeated-marker used when cell repeats.
  fo_table-cell.model =
    fo_marker*,
    axf_table-cell-repeated-marker?,
    ((block.fo.list | neutral.fo.list)* & point.fo.list*)
  # For fo:instream-foreign-object
  # Exclude axf:* elements and attributes from counting as 'non-xsl'.
  non-xsl =
    attribute * - (local:* | axf:* | xml:*) { text }*,
    element * - (local:* | fo:* | axf:*) {
      attribute * { text }*,
      anything
    }*
  # 
  # End of overrides to patterns in 'axf-inherited.rnc'.
  #
  
}
# 
# Antenna House extension elements
#

# <axf:custom-property/>
# https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.custom-property
axf_custom-property =
  
  ## Custom PDF property Effective with PDF 1.4 or later. PDF/Aでは無効です。
  element axf:custom-property {
    
    ## カスタムプロパティ名どちらも空ではいけません。
    attribute name { text },
    
    ## カスタムプロパティの値どちらも空ではいけません。
    attribute value { text },
    empty
  }
# <axf:document-info/>
# https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.document-info
axf_document-info =
  
  ## PDFには、著者やタイトルなどの文書情報を埋め込んでおくことができます 
  element axf:document-info {
    # 'name="title"' was used in V5.1 and still works but is
    # undocumented in V6.2+.
    
    ## 文字情報の名
    attribute name {
      "document-title"
      | "title"
      | "subject"
      | "author"
      | "author-title"
      | "description-writer"
      | "keywords"
      | "copyright-status"
      | "copyright-notice"
      | "copyright-info-url"
      | "xmp"
      | "pagemode"
      | "pagelayout"
      | "hidetoolbar"
      | "hidemenubar"
      | "hidewindowui"
      | "fitwindow"
      | "centerwindow"
      | "displaydoctitle"
      | "openaction"
    },
    
    ## 文字情報の値ある情は許容値の制約があります。
    attribute value { text },
    empty
  }
# <axf:footnote-number/>
# https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.footnote-number
axf_footnote-number =
  
  ## 脚注番号を生成します
  element axf:footnote-number { id, empty }
# <axf:footnote-number-citation/>
# https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.footnote-number-citation
axf_footnote-number-citation =
  
  ## 脚注番号を引用します
  element axf:footnote-number-citation {
    attribute ref-id { text },
    empty
  }
# <axf:form>
# https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.form
axf_form = element axf:form { axf_form.attlist, axf_form.model }
axf_form.attlist =
  axf_field-submit-coordinates?,
  axf_field-submit-method?,
  external-destination?
axf_form.model = block.fo.list*
# <axf:form-field>
# 
# https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.form-field
axf_form-field =
  
  ## フォームフィールドを生成します。
  element axf:form-field {
    (axf_form-field-option | axf_form-field-event)*
  }
# <axf:form-field-event>
# 
# https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.form-field-event
axf_form-field-event =
  
  ## イベントに対応したフォームアクションを定義します。
  element axf:form-field-event { text }
# <axf:form-field-option>
# 
# https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.form-field-option
axf_form-field-option =
  
  ## リストボックス、コンボボックスの構成要素を指定します。 
  element axf:form-field-option { text }
# <axf:hyphenation-info>
# https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.hyphenation-info
axf_hyphenation-info =
  
  ## Additional hyphenation information
  element axf:hyphenation-info {
    axf_hyphenation-info.attlist, axh_exceptions?
  }
axf_hyphenation-info.attlist =
  attribute language { text }?,
  attribute src { text }?
# <axf:ruby>
# https://www.antennahouse.com/product/ahf65/ahf-ruby.html#axf.ruby
axf_ruby =
  
  ## The ruby structure
  element axf:ruby { axf.ruby.attlist, (axf_ruby-base, axf_ruby-text)+ }
# <axf:ruby-base>
# https://www.antennahouse.com/product/ahf65/ahf-ruby.html#axf.ruby-base
axf_ruby-base =
  
  ## Base character of ruby
  element axf:ruby-base { text | axf_ruby }
# <axf:ruby-text>
# https://www.antennahouse.com/product/ahf65/ahf-ruby.html#axf.ruby-text
axf_ruby-text =
  
  ## Ruby
  element axf:ruby-text { text }
# <axf:spread-page-master>
# https://www.antennahouse.com/product/ahf65/ahf-spread.html#axf.spread-page-master
axf_spread-page-master =
  
  ## A two-page spread
  element axf:spread-page-master {
    axf_background-color,
    axf_background-content-height,
    axf_background-content-type,
    axf_background-content-width,
    axf_background-image,
    axf_background-position-horizontal,
    axf_background-position-vertical,
    axf_background-repeat,
    axf_background-scaling,
    axf_bleed,
    common-margin-properties-block,
    
    ## 見開きでの左ページのマスタ名
    attribute left-page-master-reference { xsd:NCName },
    master-name,
    reference-orientation,
    
    ## 見開きでの右ページのマスタ
    attribute right-page-master-reference { xsd:NCName },
    writing-mode,
    background-attachment,
    background-image,
    background-position-horizontal,
    background-position-vertical,
    background-repeat,
    axf_spread-region+
  }
# <axf:spread-region>
# https://www.antennahouse.com/product/ahf65/ahf-spread.html#axf.spread-region
axf_spread-region =
  
  ## 見開きページの領域
  element axf:spread-region {
    common-absolute-position-properties,
    common-border-padding-and-background-properties,
    common-margin-properties-block,
    height,
    region-name,
    reference-orientation,
    overflow,
    width,
    writing-mode,
    empty
  }
# <axf:tab>
# https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.tab
axf_tab =
  
  ## タブ位置までスペースを空けます
  element axf:tab { axf_tab-align, empty }
# <axf:table-cell-repeated-marker>
# https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.table-cell-repeated-marker
axf_table-cell-repeated-marker =
  
  ## 内容がaxf:repeat-cell-content-at-break="true"の場合に使われます
  element axf:table-cell-repeated-marker {
    (text | inline.fo.list | block.fo.list)*
  }
