<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://www.antennahouse.com/names/XSL/Extensions" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:axf="http://www.antennahouse.com/names/XSL/Extensions" xmlns:axs="http://www.antennahouse.com/names/XSL/Settings" xmlns:axh="http://www.antennahouse.com/names/XSL/Hyphenations">
  <xs:include schemaLocation="axf-inherited.xsd"/>
  <xs:import namespace="http://www.antennahouse.com/names/XSL/Hyphenations" schemaLocation="axh.xsd"/>
  <xs:import namespace="http://www.antennahouse.com/names/XSL/Settings" schemaLocation="axs.xsd"/>
  <xs:import namespace="http://www.w3.org/1999/XSL/Format" schemaLocation="fo.xsd"/>
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
  <!--
    
    Overrides to patterns in 'axf-inherited.rnc' (and patterns in
    modules that it includes).
    
  -->
  <!-- https://www.antennahouse.com/product/ahf65/ahf-ext.html#fo.change-bar-begin -->
  <xs:complexType name="fo_change-bar-begin.model">
    <xs:sequence>
      <xs:element minOccurs="0" ref="fo:float"/>
    </xs:sequence>
  </xs:complexType>
  <!--
    Nested fo:page-sequence
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#fo.page-sequence
  -->
  <xs:complexType name="fo_flow.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list.block"/>
          <xs:element ref="fo:page-sequence"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="fo_layout-master-set.model">
    <xs:choice maxOccurs="unbounded">
      <xs:element ref="axf:spread-page-master"/>
      <xs:element ref="fo:simple-page-master"/>
      <xs:element ref="fo:page-sequence-master"/>
      <xs:element ref="fo:flow-map"/>
    </xs:choice>
  </xs:complexType>
  <!-- https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.document-info -->
  <xs:complexType name="fo_root.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="axf:document-info"/>
      <xs:element ref="fo:layout-master-set"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="axf:document-info"/>
      <xs:element minOccurs="0" ref="fo:declarations"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="axf:document-info"/>
      <xs:element minOccurs="0" ref="fo:bookmark-tree"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="axf:document-info"/>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="fo:page-sequence"/>
        <xs:element ref="fo:page-sequence-wrapper"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- AH Formatter allows fo:region-* in any order. -->
  <xs:complexType name="fo_simple-page-master.model">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="fo:region-body"/>
      <xs:element ref="fo:region-before"/>
      <xs:element ref="fo:region-after"/>
      <xs:element ref="fo:region-start"/>
      <xs:element ref="fo:region-end"/>
    </xs:choice>
  </xs:complexType>
  <!--
    AH Formatter uses axf:custom-property and axf:document-info within
    fo:declarations.
  -->
  <xs:complexType name="fo_declarations.model">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="axf:custom-property"/>
      <xs:element ref="axf:document-info"/>
      <xs:element ref="axf:font-face"/>
      <xs:element ref="axf:formatter-config"/>
      <xs:element ref="axf:hyphenation-info"/>
      <xs:element ref="axf:space-between-digit-and-punctuation"/>
      <xs:element ref="axf:space-between-punctuation-and-digit"/>
      <xs:element ref="axf:space-end-punctuation"/>
      <xs:element ref="axf:space-start-punctuation"/>
      <xs:element ref="axf:output-volume-info"/>
    </xs:choice>
  </xs:complexType>
  <!-- AH Formatter allows empty fo:static-content. -->
  <xs:complexType name="fo_static-content.model">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:choice>
        <xs:element ref="fo:block.fo.list"/>
        <xs:group ref="fo:neutral.fo.list"/>
      </xs:choice>
      <xs:element ref="fo:point.fo.list"/>
    </xs:choice>
  </xs:complexType>
  <!-- AH Formatter allows fo:block-container to omit block FOs. -->
  <xs:complexType name="fo_block-container.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- AH Formatter allows fo:inline-container to omit block FOs. -->
  <xs:complexType name="fo_inline-container.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- AH Formatter allows empty fo:list-item. -->
  <xs:complexType name="fo_list-item.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:list-item-label"/>
          <xs:element ref="fo:list-item-body"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- AH Formatter allows fo:list-item-label omit block FOs. -->
  <xs:complexType name="fo_list-item-label.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!--
    AH Formatter allows fo:table-cell to omit block FOs.
    axf:table-cell-repeated-marker used when cell repeats.
  -->
  <xs:complexType name="fo_table-cell.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:element minOccurs="0" ref="axf:table-cell-repeated-marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!--
    For fo:instream-foreign-object
    Exclude axf:* elements and attributes from counting as 'non-xsl'.
  -->
  <xs:group name="non-xsl">
    <xs:sequence>
      <xs:any minOccurs="0" maxOccurs="unbounded" processContents="skip"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="non-xsl">
    <xs:anyAttribute processContents="skip"/>
  </xs:attributeGroup>
  <!--
    
    End of overrides to patterns in 'axf-inherited.rnc'.
    
  -->
  <!--
    
    Antenna House extension elements
    
  -->
  <!--
    <axf:custom-property/>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.custom-property
  -->
  <xs:element name="custom-property">
    <xs:annotation>
      <xs:documentation>PDF 1.4 以降のとき Custom PDF は有効です。PDF/A では無効です。</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>カスタムプロパティ名どちらも空ではいけません。</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="value" use="required">
        <xs:annotation>
          <xs:documentation>カスタムプロパティの値どちらも空ではいけません。</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:document-info/>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.document-info
  -->
  <xs:element name="document-info">
    <xs:annotation>
      <xs:documentation>PDF には、著者やタイトルなどの文書情報を埋め込んでおくことができます </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>文字情報の名</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="document-title"/>
            <xs:enumeration value="title"/>
            <xs:enumeration value="subject"/>
            <xs:enumeration value="author"/>
            <xs:enumeration value="author-title"/>
            <xs:enumeration value="description-writer"/>
            <xs:enumeration value="keywords"/>
            <xs:enumeration value="copyright-status"/>
            <xs:enumeration value="copyright-notice"/>
            <xs:enumeration value="copyright-info-url"/>
            <xs:enumeration value="xmp"/>
            <xs:enumeration value="pagemode"/>
            <xs:enumeration value="pagelayout"/>
            <xs:enumeration value="hidetoolbar"/>
            <xs:enumeration value="hidemenubar"/>
            <xs:enumeration value="hidewindowui"/>
            <xs:enumeration value="fitwindow"/>
            <xs:enumeration value="centerwindow"/>
            <xs:enumeration value="displaydoctitle"/>
            <xs:enumeration value="openaction"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="value" use="required">
        <xs:annotation>
          <xs:documentation>文字情報の値ある情は許容値の制約があります。</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:font-face>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.font-face
  -->
  <xs:element name="font-face">
    <xs:annotation>
      <xs:documentation>追加のハイフネーション情報</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="axf:axf_font-face.attlist"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="axf_font-face.attlist">
    <xs:attribute name="src" use="required"/>
    <xs:attribute name="font-family" use="required"/>
    <xs:attribute name="font-style">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="normal"/>
          <xs:enumeration value="italic"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="font-weight">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="normal"/>
          <xs:enumeration value="bold"/>
          <xs:enumeration value="100"/>
          <xs:enumeration value="200"/>
          <xs:enumeration value="300"/>
          <xs:enumeration value="400"/>
          <xs:enumeration value="500"/>
          <xs:enumeration value="600"/>
          <xs:enumeration value="700"/>
          <xs:enumeration value="800"/>
          <xs:enumeration value="900"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
    <axf:footnote-number/>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.footnote-number
  -->
  <xs:element name="footnote-number" substitutionGroup="fo:inline.fo.list">
    <xs:annotation>
      <xs:documentation>脚注番号を生成します</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="fo:id"/>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:footnote-number-citation/>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.footnote-number-citation
  -->
  <xs:element name="footnote-number-citation" substitutionGroup="fo:inline.fo.list">
    <xs:annotation>
      <xs:documentation>脚注番号を引用します</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="ref-id" use="required"/>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:form>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.form
  -->
  <xs:element name="form" substitutionGroup="fo:block.fo.list">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="axf:axf_form.model">
          <xs:attributeGroup ref="axf:axf_form.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="axf_form.attlist">
    <xs:attributeGroup ref="axf:axf_field-submit-coordinates"/>
    <xs:attributeGroup ref="axf:axf_field-submit-method"/>
    <xs:attributeGroup ref="fo:external-destination"/>
  </xs:attributeGroup>
  <xs:complexType name="axf_form.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:block.fo.list"/>
    </xs:sequence>
  </xs:complexType>
  <!--
    <axf:form-field>
    
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.form-field
  -->
  <xs:element name="form-field" substitutionGroup="fo:inline.fo.list">
    <xs:annotation>
      <xs:documentation>フォームフィールドを生成します。</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="axf:axf_form-field.model">
          <xs:attributeGroup ref="axf:axf_form-field.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="axf_form-field.model">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="axf:form-field-option"/>
      <xs:element ref="axf:form-field-event"/>
    </xs:choice>
  </xs:complexType>
  <xs:attributeGroup name="axf_form-field.attlist">
    <xs:attributeGroup ref="axf:axf_action-type.form"/>
    <xs:attributeGroup ref="axf:axf_field-button-face"/>
    <xs:attributeGroup ref="axf:axf_field-button-face-down"/>
    <xs:attributeGroup ref="axf:axf_field-button-face-rollover"/>
    <xs:attributeGroup ref="axf:axf_field-button-icon"/>
    <xs:attributeGroup ref="axf:axf_field-button-icon-down"/>
    <xs:attributeGroup ref="axf:axf_field-button-icon-rollover"/>
    <xs:attributeGroup ref="axf:axf_field-button-layout"/>
    <xs:attributeGroup ref="axf:axf_field-checked"/>
    <xs:attributeGroup ref="axf:axf_field-checked-style"/>
    <xs:attributeGroup ref="axf:axf_field-default-text"/>
    <xs:attributeGroup ref="axf:axf_field-description"/>
    <xs:attributeGroup ref="axf:axf_field-editable"/>
    <xs:attributeGroup ref="axf:axf_field-flags"/>
    <xs:attributeGroup ref="axf:axf_field-font-size"/>
    <xs:attributeGroup ref="axf:axf_field-format"/>
    <xs:attributeGroup ref="axf:axf_field-format-category"/>
    <xs:attributeGroup ref="axf:axf_field-maxlen"/>
    <xs:attributeGroup ref="axf:axf_field-multiline"/>
    <xs:attributeGroup ref="axf:axf_field-multiple"/>
    <xs:attributeGroup ref="axf:axf_field-name"/>
    <xs:attributeGroup ref="axf:axf_field-name-suffix-page-number"/>
    <xs:attributeGroup ref="axf:axf_field-scroll"/>
    <xs:attributeGroup ref="axf:axf_field-text-align"/>
    <xs:attributeGroup ref="axf:axf_field-top-index"/>
    <xs:attributeGroup ref="axf:axf_field-type"/>
    <xs:attributeGroup ref="axf:axf_field-value"/>
    <xs:attributeGroup ref="fo:common-border-padding-and-background-properties"/>
    <xs:attributeGroup ref="fo:external-destination"/>
    <xs:attributeGroup ref="fo:height"/>
    <xs:attributeGroup ref="fo:width"/>
  </xs:attributeGroup>
  <!--
    <axf:form-field-event>
    
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.form-field-event
  -->
  <xs:element name="form-field-event">
    <xs:annotation>
      <xs:documentation>イベントに対応したフォームアクションを定義します。</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:attributeGroup ref="axf:axf_form-field-event.attlist"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="axf_form-field-event.attlist">
    <xs:attribute name="name" form="qualified">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="MouseUp"/>
          <xs:enumeration value="MouseDown"/>
          <xs:enumeration value="MouseEnter"/>
          <xs:enumeration value="MouseExit"/>
          <xs:enumeration value="OnFocus"/>
          <xs:enumeration value="OnBlur"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="name">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="MouseUp"/>
          <xs:enumeration value="MouseDown"/>
          <xs:enumeration value="MouseEnter"/>
          <xs:enumeration value="MouseExit"/>
          <xs:enumeration value="OnFocus"/>
          <xs:enumeration value="OnBlur"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="action-type" form="qualified">
      <xs:simpleType>
        <xs:union memberTypes="axf:axf_action-type.form.model">
          <xs:simpleType>
            <xs:restriction base="xs:token">
              <xs:enumeration value="javascript"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:union>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="action-type">
      <xs:simpleType>
        <xs:union memberTypes="axf:axf_action-type.form.model">
          <xs:simpleType>
            <xs:restriction base="xs:token">
              <xs:enumeration value="javascript"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:union>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
    <axf:form-field-option>
    
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.form-field-option
  -->
  <xs:element name="form-field-option">
    <xs:annotation>
      <xs:documentation>リストボックス、コンボボックスの構成要素を指定します。 </xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:attributeGroup ref="axf:axf_form-field-option.attlist"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="axf_form-field-option.attlist">
    <xs:attributeGroup ref="axf:axf_field-value"/>
  </xs:attributeGroup>
  <!--
    <axf:formatter-config>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.formatter-config
  -->
  <xs:element name="formatter-config">
    <xs:annotation>
      <xs:documentation>追加のハイフネーション情報</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="axs:formatter-config.model">
          <xs:attributeGroup ref="axf:axf_formatter-config.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <!--
    axf_formatter-config.model =
      axs_pdf-settings?
  -->
  <xs:attributeGroup name="axf_formatter-config.attlist">
    <xs:attribute name="src">
      <xs:annotation>
        <xs:documentation>外部にあるオプション設定ファイル</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
    <axf:hyphenation-info>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.hyphenation-info
  -->
  <xs:element name="hyphenation-info">
    <xs:annotation>
      <xs:documentation>追加のハイフネーション情報</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="axh:exceptions"/>
      </xs:sequence>
      <xs:attributeGroup ref="axf:axf_hyphenation-info.attlist"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="axf_hyphenation-info.attlist">
    <xs:attribute name="language"/>
    <xs:attribute name="src"/>
  </xs:attributeGroup>
  <!--
    <axf:output-volume-info>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.output-volume-info
  -->
  <xs:element name="output-volume-info">
    <xs:annotation>
      <xs:documentation>多分冊出力指定</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="axf:axf_output-volume-info.attlist"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="axf_output-volume-info.attlist">
    <xs:attribute name="initial-volume-number" type="xs:positiveInteger">
      <xs:annotation>
        <xs:documentation>初期文書番号： &lt;positive-integer&gt;</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="format">
      <xs:annotation>
        <xs:documentation>書式指定： &lt;string&gt;</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="bookmark-include">
      <xs:annotation>
        <xs:documentation>多分冊にしおりを付ける方法： first | all | separate</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="first"/>
          <xs:enumeration value="all"/>
          <xs:enumeration value="separate"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
    <axf:ruby>
    https://www.antennahouse.com/product/ahf65/ahf-ruby.html#axf.ruby
  -->
  <xs:element name="ruby" substitutionGroup="fo:inline.fo.list">
    <xs:annotation>
      <xs:documentation>ルビ構造</xs:documentation>
