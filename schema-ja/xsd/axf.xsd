<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://www.antennahouse.com/names/XSL/Extensions" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:axf="http://www.antennahouse.com/names/XSL/Extensions">
  <xs:include schemaLocation="axf-inherited.xsd"/>
  <xs:import namespace="http://www.w3.org/1999/XSL/Format" schemaLocation="fo.xsd"/>
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
  <!--
    
    Overrides to patterns in 'fo-inherited.rnc' (and patterns in
    modules that it includes).
    
  -->
  <!-- http://www.antennahouse.com/product/ahf64/ahf-ext.html#fo.change-bar-begin -->
  <xs:complexType name="fo_change-bar-begin.model">
    <xs:sequence>
      <xs:element minOccurs="0" ref="fo:float"/>
    </xs:sequence>
  </xs:complexType>
  <!--
    Nested fo:page-sequence
    https://www.antennahouse.com/product/ahf64/ahf-ext.html#fo.page-sequence
  -->
  <xs:complexType name="fo_flow.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list.block"/>
          <xs:element ref="fo:page-sequence"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="fo_layout-master-set.model">
    <xs:choice maxOccurs="unbounded">
      <xs:element ref="axf:spread-page-master"/>
      <xs:element ref="fo:simple-page-master"/>
      <xs:element ref="fo:page-sequence-master"/>
      <xs:element ref="fo:flow-map"/>
    </xs:choice>
  </xs:complexType>
  <!-- http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.document-info -->
  <xs:complexType name="fo_root.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="axf:document-info"/>
      <xs:element ref="fo:layout-master-set"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="axf:document-info"/>
      <xs:element minOccurs="0" ref="fo:declarations"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="axf:document-info"/>
      <xs:element minOccurs="0" ref="fo:bookmark-tree"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="axf:document-info"/>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="fo:page-sequence"/>
        <xs:element ref="fo:page-sequence-wrapper"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- AH Formatter allows fo:region-* in any order. -->
  <xs:complexType name="fo_simple-page-master.model">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="fo:region-body"/>
      <xs:element ref="fo:region-before"/>
      <xs:element ref="fo:region-after"/>
      <xs:element ref="fo:region-start"/>
      <xs:element ref="fo:region-end"/>
    </xs:choice>
  </xs:complexType>
  <!-- AH Formatter allows empty fo:static-content. -->
  <xs:complexType name="fo_static-content.model">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:choice>
        <xs:element ref="fo:block.fo.list"/>
        <xs:group ref="fo:neutral.fo.list"/>
      </xs:choice>
      <xs:element ref="fo:point.fo.list"/>
    </xs:choice>
  </xs:complexType>
  <!-- AH Formatter allows fo:block-container to omit block FOs. -->
  <xs:complexType name="fo_block-container.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- AH Formatter allows fo:inline-container to omit block FOs. -->
  <xs:complexType name="fo_inline-container.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- AH Formatter allows empty fo:list-item. -->
  <xs:complexType name="fo_list-item.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:list-item-label"/>
          <xs:element ref="fo:list-item-body"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- AH Formatter allows fo:list-item-label omit block FOs. -->
  <xs:complexType name="fo_list-item-label.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!--
    AH Formatter allows fo:table-cell to omit block FOs.
    axf:table-cell-repeated-marker used when cell repeats.
  -->
  <xs:complexType name="fo_table-cell.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:element minOccurs="0" ref="axf:table-cell-repeated-marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!--
    For fo:instream-foreign-object
    Exclude axf:* elements and attributes from counting as 'non-xsl'.
  -->
  <xs:group name="non-xsl">
    <xs:sequence>
      <xs:any minOccurs="0" maxOccurs="unbounded" processContents="skip"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="non-xsl">
    <xs:anyAttribute processContents="skip"/>
  </xs:attributeGroup>
  <!--
    
    End of overrides to patterns in 'axf-inherited.rnc'.
    
  -->
  <!--
    
    Antenna House extension elements
    
  -->
  <!--
    <axf:document-info/>
    http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.document-info
  -->
  <xs:element name="document-info">
    <xs:annotation>
      <xs:documentation>PDFには、著者やタイトルなどの文書情報を埋め込んでおくことができます </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>文字情報の名</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="document-title"/>
            <xs:enumeration value="title"/>
            <xs:enumeration value="subject"/>
            <xs:enumeration value="author"/>
            <xs:enumeration value="author-title"/>
            <xs:enumeration value="description-writer"/>
            <xs:enumeration value="keywords"/>
            <xs:enumeration value="copyright-status"/>
            <xs:enumeration value="copyright-notice"/>
            <xs:enumeration value="copyright-info-url"/>
            <xs:enumeration value="xmp"/>
            <xs:enumeration value="pagemode"/>
            <xs:enumeration value="pagelayout"/>
            <xs:enumeration value="hidetoolbar"/>
            <xs:enumeration value="hidemenubar"/>
            <xs:enumeration value="hidewindowui"/>
            <xs:enumeration value="fitwindow"/>
            <xs:enumeration value="centerwindow"/>
            <xs:enumeration value="displaydoctitle"/>
            <xs:enumeration value="openaction"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="value" use="required">
        <xs:annotation>
          <xs:documentation>文字情報の値ある情は許容値の制約があります。</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:footnote-number/>
    http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.footnote-number
  -->
  <xs:element name="footnote-number" substitutionGroup="fo:inline.fo.list">
    <xs:annotation>
      <xs:documentation>脚注番号を生成します</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="fo:id"/>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:footnote-number-citation/>
    http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.footnote-number-citation
  -->
  <xs:element name="footnote-number-citation" substitutionGroup="fo:inline.fo.list">
    <xs:annotation>
      <xs:documentation>脚注番号を引用します</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="ref-id" use="required"/>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:form>
    https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.form
  -->
  <xs:element name="form" substitutionGroup="fo:block.fo.list">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="axf:axf_form.model">
          <xs:attributeGroup ref="axf:axf_form.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="axf_form.attlist">
    <xs:attributeGroup ref="axf:axf_field-submit-coordinates"/>
    <xs:attributeGroup ref="axf:axf_field-submit-method"/>
    <xs:attributeGroup ref="fo:external-destination"/>
  </xs:attributeGroup>
  <xs:complexType name="axf_form.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:block.fo.list"/>
    </xs:sequence>
  </xs:complexType>
  <!--
    <axf:form-field>
    
    https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.form-field
  -->
  <xs:element name="form-field" substitutionGroup="fo:inline.fo.list">
    <xs:annotation>
      <xs:documentation>Generates a form field</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="axf:form-field-option"/>
        <xs:element ref="axf:form-field-event"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:form-field-event>
    
    https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.form-field-event
  -->
  <xs:element name="form-field-event" type="xs:string">
    <xs:annotation>
      <xs:documentation>Defines the form action corresponding to an event</xs:documentation>
    </xs:annotation>
  </xs:element>
  <!--
    <axf:form-field-option>
    
    https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.form-field-option
  -->
  <xs:element name="form-field-option" type="xs:string">
    <xs:annotation>
      <xs:documentation>Specifies the component elements of the list box, combo box</xs:documentation>
    </xs:annotation>
  </xs:element>
  <!--
    <axf:ruby>
    https://www.antennahouse.com/product/ahf64/ahf-ruby.html#axf.ruby
  -->
  <xs:element name="ruby" substitutionGroup="fo:inline.fo.list">
    <xs:annotation>
      <xs:documentation>The ruby structure.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence maxOccurs="unbounded">
        <xs:element ref="axf:ruby-base"/>
        <xs:element ref="axf:ruby-text"/>
      </xs:sequence>
      <xs:attributeGroup ref="axf:axf.ruby.attlist"/>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:ruby-base>
    https://www.antennahouse.com/product/ahf64/ahf-ruby.html#axf.ruby-base
  -->
  <xs:element name="ruby-base">
    <xs:annotation>
      <xs:documentation>Base character of ruby.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" ref="axf:ruby"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:ruby-text>
    https://www.antennahouse.com/product/ahf64/ahf-ruby.html#axf.ruby-text
  -->
  <xs:element name="ruby-text" type="xs:string">
    <xs:annotation>
      <xs:documentation>Ruby.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <!--
    <axf:spread-page-master>
    http://www.antennahouse.com/product/ahf64/ahf-spread.html#axf.spread-page-master
  -->
  <xs:element name="spread-page-master">
    <xs:annotation>
      <xs:documentation>見開きページマスタ</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="axf:spread-region"/>
      </xs:sequence>
      <xs:attributeGroup ref="axf:axf_background-color"/>
      <xs:attributeGroup ref="axf:axf_background-content-height"/>
      <xs:attributeGroup ref="axf:axf_background-content-type"/>
      <xs:attributeGroup ref="axf:axf_background-content-width"/>
      <xs:attributeGroup ref="axf:axf_background-image"/>
      <xs:attributeGroup ref="axf:axf_background-position-horizontal"/>
      <xs:attributeGroup ref="axf:axf_background-position-vertical"/>
      <xs:attributeGroup ref="axf:axf_background-repeat"/>
      <xs:attributeGroup ref="axf:axf_background-scaling"/>
      <xs:attributeGroup ref="axf:axf_bleed"/>
      <xs:attributeGroup ref="fo:common-margin-properties-block"/>
      <xs:attribute name="left-page-master-reference" use="required" type="xs:NCName">
        <xs:annotation>
          <xs:documentation>Master name of the page master for the left page of a two-page spread.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attributeGroup ref="fo:master-name"/>
      <xs:attributeGroup ref="fo:reference-orientation"/>
      <xs:attribute name="right-page-master-reference" use="required" type="xs:NCName">
        <xs:annotation>
          <xs:documentation>Master name of the page master for the right page of a two-page spread.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attributeGroup ref="fo:writing-mode"/>
      <xs:attributeGroup ref="fo:background-attachment"/>
      <xs:attributeGroup ref="fo:background-image"/>
      <xs:attributeGroup ref="fo:background-position-horizontal"/>
      <xs:attributeGroup ref="fo:background-position-vertical"/>
      <xs:attributeGroup ref="fo:background-repeat"/>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:spread-region>
    http://www.antennahouse.com/product/ahf64/ahf-spread.html#axf.spread-region
  -->
  <xs:element name="spread-region">
    <xs:annotation>
      <xs:documentation>見開きページの領域</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="fo:common-absolute-position-properties"/>
      <xs:attributeGroup ref="fo:common-border-padding-and-background-properties"/>
      <xs:attributeGroup ref="fo:common-margin-properties-block"/>
      <xs:attributeGroup ref="fo:height"/>
      <xs:attributeGroup ref="fo:region-name"/>
      <xs:attributeGroup ref="fo:reference-orientation"/>
      <xs:attributeGroup ref="fo:overflow"/>
      <xs:attributeGroup ref="fo:width"/>
      <xs:attributeGroup ref="fo:writing-mode"/>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:tab>
    https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.tab
  -->
  <xs:element name="tab" substitutionGroup="fo:inline.fo.list">
    <xs:annotation>
      <xs:documentation>タブ位置までスペースを空けます</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="axf:axf_tab-align"/>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:table-cell-repeated-marker>
    http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.table-cell-repeated-marker
  -->
  <xs:element name="table-cell-repeated-marker">
    <xs:annotation>
      <xs:documentation>内容がaxf:repeat-cell-content-at-break="true"の場合に使われます</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="fo:inline.fo.list"/>
        <xs:element ref="fo:block.fo.list"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--
    
    Antenna House extensions to patterns in included modules.
    
  -->
  <!--
    
    Extensions to FO models
    
  -->
  <!--
    
    Extensions to FO attlists
    
  -->
  <!-- FOs that are not descendants of fo:flow -->
  <!-- FOs in flow listed in alphabetic order -->
  <!--
    
    Extensions to property groups
    
  -->
  <!--
    
    Antenna House extension properties
    
  -->
  <!--
    List of annotation-related properties common to block-level and
    inline-level formatting objects
  -->
  <xs:attributeGroup name="axf.annotation.attlist">
    <xs:attributeGroup ref="axf:axf_annotation-author"/>
    <xs:attributeGroup ref="axf:axf_annotation-color"/>
    <xs:attributeGroup ref="axf:axf_annotation-contents"/>
    <xs:attributeGroup ref="axf:axf_annotation-file-attachment"/>
    <xs:attributeGroup ref="axf:axf_annotation-height"/>
    <xs:attributeGroup ref="axf:axf_annotation-icon-name"/>
    <xs:attributeGroup ref="axf:axf_annotation-open"/>
    <xs:attributeGroup ref="axf:axf_annotation-position-horizontal"/>
    <xs:attributeGroup ref="axf:axf_annotation-position-vertical"/>
    <xs:attributeGroup ref="axf:axf_annotation-title"/>
    <xs:attributeGroup ref="axf:axf_annotation-type"/>
    <xs:attributeGroup ref="axf:axf_annotation-width"/>
  </xs:attributeGroup>
  <!--
    List of baseline-grid-related extension properties
    https://www.antennahouse.com/product/ahf64/ahf-ext.html#https://www.antennahouse.com/product/ahf64/ahf-ext.html
  -->
  <xs:attributeGroup name="axf-baseline-grid-properties">
    <xs:attributeGroup ref="axf:axf_baseline-grid"/>
    <xs:attributeGroup ref="axf:axf_baseline-block-snap"/>
  </xs:attributeGroup>
  <!--
    List of outline-related extension properties
    https://www.antennahouse.com/product/ahf64/ahf-ext.html#bookmark-and-link
  -->
  <xs:attributeGroup name="axf-outline-properties">
    <xs:attributeGroup ref="axf:axf_outline-expand"/>
    <xs:attributeGroup ref="axf:axf_outline-group"/>
    <xs:attributeGroup ref="axf:axf_outline-level"/>
    <xs:attributeGroup ref="axf:axf_outline-title"/>
    <xs:attributeGroup ref="axf:axf_outline-color"/>
    <xs:attributeGroup ref="axf:axf_outline-font-style"/>
    <xs:attributeGroup ref="axf:axf_outline-font-weight"/>
  </xs:attributeGroup>
  <!--
    List of extension properties common to all block-level formatting
    objects
  -->
  <xs:attributeGroup name="axf.block.attlist">
    <xs:attributeGroup ref="axf:axf-baseline-grid-properties"/>
    <xs:attributeGroup ref="axf:axf-outline-properties"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="axf.ruby.attlist">
    <xs:attributeGroup ref="axf:axf_ruby-align"/>
    <xs:attributeGroup ref="axf:axf_ruby-position"/>
    <xs:attributeGroup ref="axf:axf_ruby-offset"/>
    <xs:attributeGroup ref="axf:axf_ruby-overhang"/>
    <xs:attributeGroup ref="axf:axf_ruby-limit-overhang"/>
    <xs:attributeGroup ref="axf:axf_ruby-limit-space"/>
    <xs:attributeGroup ref="axf:axf_ruby-small-kana"/>
    <xs:attributeGroup ref="axf:axf_ruby-font-family"/>
    <xs:attributeGroup ref="axf:axf_ruby-font-size"/>
    <xs:attributeGroup ref="axf:axf_ruby-minimum-font-size"/>
    <xs:attributeGroup ref="axf:axf_ruby-font-style"/>
    <xs:attributeGroup ref="axf:axf_ruby-font-weight"/>
    <xs:attributeGroup ref="axf:axf_ruby-font-stretch"/>
    <xs:attributeGroup ref="axf:axf_ruby-condense"/>
    <xs:attributeGroup ref="axf:axf_ruby-color"/>
  </xs:attributeGroup>
  <!--
    @axf:background-color
    @axf:background-image
    @axf:background-position-horizontal
    @axf:background-position-vertical
    @axf:background-repeat
    
    http://www.antennahouse.com/product/ahf64/ahf-ext.html#IDAAOLV
  -->
  <xs:attributeGroup name="axf_background-color">
    <xs:attribute name="background-color" form="qualified">
      <xs:annotation>
        <xs:documentation>&lt;color&gt; | transparent</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="axf_background-image">
    <xs:attribute name="background-image" form="qualified">
      <xs:annotation>
        <xs:documentation>&lt;uri-specification&gt; | none</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="axf_background-position-horizontal">
    <xs:attribute name="background-position-horizontal" form="qualified">
      <xs:annotation>
        <xs:documentation>&lt;percentage&gt; | &lt;length&gt; | left | center | right</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="axf_background-position-vertical">
    <xs:attribute name="background-position-vertical" form="qualified">
      <xs:annotation>
        <xs:documentation>&lt;percentage&gt; | &lt;length&gt; | top | center | bottom</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="axf_background-repeat">
    <xs:attribute name="background-repeat" form="qualified">
      <xs:annotation>
        <xs:documentation>repeat | repeat-x | repeat-y | no-repeat | paginate</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
    @axf:action-type
    
    http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.action-type
  -->
  <xs:attributeGroup name="axf_action-type">
    <xs:attribute name="action-type" form="qualified">
      <xs:annotation>
        <xs:documentation>外部リンクまたはフォームアクションの動作を指定します</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
