<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://www.antennahouse.com/names/XSL/Extensions" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:axf="http://www.antennahouse.com/names/XSL/Extensions" xmlns:axs="http://www.antennahouse.com/names/XSL/Settings" xmlns:axh="http://www.antennahouse.com/names/XSL/Hyphenations">
  <xs:include schemaLocation="axf-inherited.xsd"/>
  <xs:import namespace="http://www.antennahouse.com/names/XSL/Hyphenations" schemaLocation="axh.xsd"/>
  <xs:import namespace="http://www.antennahouse.com/names/XSL/Settings" schemaLocation="ahfsettings.xsd"/>
  <xs:import namespace="http://www.w3.org/1999/XSL/Format" schemaLocation="fo.xsd"/>
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
  <!--
    
    Overrides to patterns in 'axf-inherited.rnc' (and patterns in
    modules that it includes).
    
  -->
  <!-- https://www.antennahouse.com/product/ahf65/ahf-ext.html#fo.change-bar-begin -->
  <xs:complexType name="fo_change-bar-begin.model">
    <xs:sequence>
      <xs:element minOccurs="0" ref="fo:float"/>
    </xs:sequence>
  </xs:complexType>
  <!--
    Nested fo:page-sequence
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#fo.page-sequence
  -->
  <xs:complexType name="fo_flow.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list.block"/>
          <xs:element ref="fo:page-sequence"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="fo_layout-master-set.model">
    <xs:choice maxOccurs="unbounded">
      <xs:element ref="axf:spread-page-master"/>
      <xs:element ref="fo:simple-page-master"/>
      <xs:element ref="fo:page-sequence-master"/>
      <xs:element ref="fo:flow-map"/>
    </xs:choice>
  </xs:complexType>
  <!-- https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.document-info -->
  <xs:complexType name="fo_root.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="axf:document-info"/>
      <xs:element ref="fo:layout-master-set"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="axf:document-info"/>
      <xs:element minOccurs="0" ref="fo:declarations"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="axf:document-info"/>
      <xs:element minOccurs="0" ref="fo:bookmark-tree"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="axf:document-info"/>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="fo:page-sequence"/>
        <xs:element ref="fo:page-sequence-wrapper"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- AH Formatter allows fo:region-* in any order. -->
  <xs:complexType name="fo_simple-page-master.model">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="fo:region-body"/>
      <xs:element ref="fo:region-before"/>
      <xs:element ref="fo:region-after"/>
      <xs:element ref="fo:region-start"/>
      <xs:element ref="fo:region-end"/>
    </xs:choice>
  </xs:complexType>
  <!--
    AH Formatter uses axf:custom-property and axf:document-info within
    fo:declarations.
  -->
  <xs:complexType name="fo_declarations.model">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="axf:custom-property"/>
      <xs:element ref="axf:document-info"/>
      <xs:element ref="axf:hyphenation-info"/>
      <xs:element ref="axf:formatter-config"/>
      <xs:element ref="axf:output-volume-info"/>
    </xs:choice>
  </xs:complexType>
  <!-- AH Formatter allows empty fo:static-content. -->
  <xs:complexType name="fo_static-content.model">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:choice>
        <xs:element ref="fo:block.fo.list"/>
        <xs:group ref="fo:neutral.fo.list"/>
      </xs:choice>
      <xs:element ref="fo:point.fo.list"/>
    </xs:choice>
  </xs:complexType>
  <!-- AH Formatter allows fo:block-container to omit block FOs. -->
  <xs:complexType name="fo_block-container.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- AH Formatter allows fo:inline-container to omit block FOs. -->
  <xs:complexType name="fo_inline-container.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- AH Formatter allows empty fo:list-item. -->
  <xs:complexType name="fo_list-item.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:list-item-label"/>
          <xs:element ref="fo:list-item-body"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- AH Formatter allows fo:list-item-label omit block FOs. -->
  <xs:complexType name="fo_list-item-label.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!--
    AH Formatter allows fo:table-cell to omit block FOs.
    axf:table-cell-repeated-marker used when cell repeats.
  -->
  <xs:complexType name="fo_table-cell.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:marker"/>
      <xs:element minOccurs="0" ref="axf:table-cell-repeated-marker"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:choice>
          <xs:element ref="fo:block.fo.list"/>
          <xs:group ref="fo:neutral.fo.list"/>
        </xs:choice>
        <xs:element ref="fo:point.fo.list"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!--
    For fo:instream-foreign-object
    Exclude axf:* elements and attributes from counting as 'non-xsl'.
  -->
  <xs:group name="non-xsl">
    <xs:sequence>
      <xs:any minOccurs="0" maxOccurs="unbounded" processContents="skip"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="non-xsl">
    <xs:anyAttribute processContents="skip"/>
  </xs:attributeGroup>
  <!--
    
    End of overrides to patterns in 'axf-inherited.rnc'.
    
  -->
  <!--
    
    Antenna House extension elements
    
  -->
  <!--
    <axf:custom-property/>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.custom-property
  -->
  <xs:element name="custom-property">
    <xs:annotation>
      <xs:documentation>Custom PDF property Effective with PDF 1.4 or later. Invalid with PDF/A</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Name of the custom property.  Should not be empty.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="value" use="required">
        <xs:annotation>
          <xs:documentation>Value of the custom property.  Should not be empty.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:document-info/>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.document-info
  -->
  <xs:element name="document-info">
    <xs:annotation>
      <xs:documentation>Document information such as an author and title that can be embedded in the PDF</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Name of the document information</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="document-title"/>
            <xs:enumeration value="title"/>
            <xs:enumeration value="subject"/>
            <xs:enumeration value="author"/>
            <xs:enumeration value="author-title"/>
            <xs:enumeration value="description-writer"/>
            <xs:enumeration value="keywords"/>
            <xs:enumeration value="copyright-status"/>
            <xs:enumeration value="copyright-notice"/>
            <xs:enumeration value="copyright-info-url"/>
            <xs:enumeration value="xmp"/>
            <xs:enumeration value="pagemode"/>
            <xs:enumeration value="pagelayout"/>
            <xs:enumeration value="hidetoolbar"/>
            <xs:enumeration value="hidemenubar"/>
            <xs:enumeration value="hidewindowui"/>
            <xs:enumeration value="fitwindow"/>
            <xs:enumeration value="centerwindow"/>
            <xs:enumeration value="displaydoctitle"/>
            <xs:enumeration value="openaction"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="value" use="required">
        <xs:annotation>
          <xs:documentation>Value of the document information.  Some information has constraints on the allowed values.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:footnote-number/>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.footnote-number
  -->
  <xs:element name="footnote-number" substitutionGroup="fo:inline.fo.list">
    <xs:annotation>
      <xs:documentation>Generates a footnote number</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="fo:id"/>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:footnote-number-citation/>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.footnote-number-citation
  -->
  <xs:element name="footnote-number-citation" substitutionGroup="fo:inline.fo.list">
    <xs:annotation>
      <xs:documentation>Cites a footnote number</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="ref-id" use="required"/>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:form>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.form
  -->
  <xs:element name="form" substitutionGroup="fo:block.fo.list">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="axf:axf_form.model">
          <xs:attributeGroup ref="axf:axf_form.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="axf_form.attlist">
    <xs:attributeGroup ref="axf:axf_field-submit-coordinates"/>
    <xs:attributeGroup ref="axf:axf_field-submit-method"/>
    <xs:attributeGroup ref="fo:external-destination"/>
  </xs:attributeGroup>
  <xs:complexType name="axf_form.model">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="fo:block.fo.list"/>
    </xs:sequence>
  </xs:complexType>
  <!--
    <axf:form-field>
    
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.form-field
  -->
  <xs:element name="form-field" substitutionGroup="fo:inline.fo.list">
    <xs:annotation>
      <xs:documentation>Generates a form field</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="axf:form-field-option"/>
        <xs:element ref="axf:form-field-event"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:form-field-event>
    
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.form-field-event
  -->
  <xs:element name="form-field-event" type="xs:string">
    <xs:annotation>
      <xs:documentation>Defines the form action corresponding to an event</xs:documentation>
    </xs:annotation>
  </xs:element>
  <!--
    <axf:form-field-option>
    
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.form-field-option
  -->
  <xs:element name="form-field-option" type="xs:string">
    <xs:annotation>
      <xs:documentation>Specifies the component elements of the list box, combo box</xs:documentation>
    </xs:annotation>
  </xs:element>
  <!--
    <axf:formatter-config>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.formatter-config
  -->
  <xs:element name="formatter-config">
    <xs:annotation>
      <xs:documentation>Additional option setting information</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="axs:formatter-config.model">
          <xs:attributeGroup ref="axf:axf_formatter-config.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <!--
    axf_formatter-config.model =
      axs_pdf-settings?
  -->
  <xs:attributeGroup name="axf_formatter-config.attlist">
    <xs:attribute name="src">
      <xs:annotation>
        <xs:documentation>External Option Setting File</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
    <axf:hyphenation-info>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.hyphenation-info
  -->
  <xs:element name="hyphenation-info">
    <xs:annotation>
      <xs:documentation>Additional hyphenation information</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="axh:exceptions"/>
      </xs:sequence>
      <xs:attributeGroup ref="axf:axf_hyphenation-info.attlist"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="axf_hyphenation-info.attlist">
    <xs:attribute name="language"/>
    <xs:attribute name="src"/>
  </xs:attributeGroup>
  <!--
    <axf:output-volume-info>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.output-volume-info
  -->
  <xs:element name="output-volume-info">
    <xs:annotation>
      <xs:documentation>Output volume specification</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="axf:axf_output-volume-info.attlist"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="axf_output-volume-info.attlist">
    <xs:attribute name="initial-volume-number" type="xs:positiveInteger">
      <xs:annotation>
        <xs:documentation>Initial volume number: &lt;positive-integer&gt;</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="format">
      <xs:annotation>
        <xs:documentation>Format specification: &lt;string&gt;</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="bookmark-include">
      <xs:annotation>
        <xs:documentation>How to include bookmarks in multiple separate volumes: first | all | separate</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="first"/>
          <xs:enumeration value="all"/>
          <xs:enumeration value="separate"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
    <axf:ruby>
    https://www.antennahouse.com/product/ahf65/ahf-ruby.html#axf.ruby
  -->
  <xs:element name="ruby" substitutionGroup="fo:inline.fo.list">
    <xs:annotation>
      <xs:documentation>The ruby structure</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence maxOccurs="unbounded">
        <xs:element ref="axf:ruby-base"/>
        <xs:element ref="axf:ruby-text"/>
      </xs:sequence>
      <xs:attributeGroup ref="axf:axf.ruby.attlist"/>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:ruby-base>
    https://www.antennahouse.com/product/ahf65/ahf-ruby.html#axf.ruby-base
  -->
  <xs:element name="ruby-base">
    <xs:annotation>
      <xs:documentation>Base character of ruby</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" ref="axf:ruby"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:ruby-text>
    https://www.antennahouse.com/product/ahf65/ahf-ruby.html#axf.ruby-text
  -->
  <xs:element name="ruby-text" type="xs:string">
    <xs:annotation>
      <xs:documentation>Ruby</xs:documentation>
    </xs:annotation>
  </xs:element>
  <!--
    <axf:spread-page-master>
    https://www.antennahouse.com/product/ahf65/ahf-spread.html#axf.spread-page-master
  -->
  <xs:element name="spread-page-master">
    <xs:annotation>
      <xs:documentation>A two-page spread</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="axf:spread-region"/>
      </xs:sequence>
      <xs:attributeGroup ref="axf:axf_background-color"/>
      <xs:attributeGroup ref="axf:axf_background-content-height"/>
      <xs:attributeGroup ref="axf:axf_background-content-type"/>
      <xs:attributeGroup ref="axf:axf_background-content-width"/>
      <xs:attributeGroup ref="axf:axf_background-image"/>
      <xs:attributeGroup ref="axf:axf_background-position-horizontal"/>
      <xs:attributeGroup ref="axf:axf_background-position-vertical"/>
      <xs:attributeGroup ref="axf:axf_background-repeat"/>
      <xs:attributeGroup ref="axf:axf_background-scaling"/>
      <xs:attributeGroup ref="axf:axf_bleed"/>
      <xs:attributeGroup ref="fo:common-margin-properties-block"/>
      <xs:attribute name="left-page-master-reference" use="required" type="xs:NCName">
        <xs:annotation>
          <xs:documentation>Master name of the page master for the left page of a two-page spread.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attributeGroup ref="fo:master-name"/>
      <xs:attributeGroup ref="fo:reference-orientation"/>
      <xs:attribute name="right-page-master-reference" use="required" type="xs:NCName">
        <xs:annotation>
          <xs:documentation>Master name of the page master for the right page of a two-page spread.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attributeGroup ref="fo:writing-mode"/>
      <xs:attributeGroup ref="fo:background-attachment"/>
      <xs:attributeGroup ref="fo:background-image"/>
      <xs:attributeGroup ref="fo:background-position-horizontal"/>
      <xs:attributeGroup ref="fo:background-position-vertical"/>
      <xs:attributeGroup ref="fo:background-repeat"/>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:spread-region>
    https://www.antennahouse.com/product/ahf65/ahf-spread.html#axf.spread-region
  -->
  <xs:element name="spread-region">
    <xs:annotation>
      <xs:documentation>Region in a two-page spread.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="fo:common-absolute-position-properties"/>
      <xs:attributeGroup ref="fo:common-border-padding-and-background-properties"/>
      <xs:attributeGroup ref="fo:common-margin-properties-block"/>
      <xs:attributeGroup ref="fo:height"/>
      <xs:attributeGroup ref="fo:region-name"/>
      <xs:attributeGroup ref="fo:reference-orientation"/>
      <xs:attributeGroup ref="fo:overflow"/>
      <xs:attributeGroup ref="fo:width"/>
      <xs:attributeGroup ref="fo:writing-mode"/>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:tab>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.tab
  -->
  <xs:element name="tab" substitutionGroup="fo:inline.fo.list">
    <xs:annotation>
      <xs:documentation>Places a white space up to a tab stop position</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="axf:axf_tab-align"/>
    </xs:complexType>
  </xs:element>
  <!--
    <axf:table-cell-repeated-marker>
    https://www.antennahouse.com/product/ahf65/ahf-ext.html#axf.table-cell-repeated-marker
  -->
  <xs:element name="table-cell-repeated-marker">
    <xs:annotation>
      <xs:documentation>Contents used when axf:repeat-cell-content-at-break="true"</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="fo:inline.fo.list"/>
        <xs:element ref="fo:block.fo.list"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--
    
    Antenna House extensions to patterns in included modules.
    
  -->
  <!--
    
    Extensions to FO models
    
  -->
  <!--
    
    Extensions to FO attlists
    
  -->
  <!-- FOs that are not descendants of fo:flow -->
  <!-- FOs in flow listed in alphabetic order -->
  <!--
    
    Extensions to property groups
    
  -->
  <!--
    
    Antenna House extension properties
    
  -->
  <!--
    List of annotation-related properties common to block-level and
    inline-level formatting objects
  -->
  <xs:attributeGroup name="axf.annotation.attlist">
    <xs:attributeGroup ref="axf:axf_annotation-author"/>
    <xs:attributeGroup ref="axf:axf_annotation-border-color"/>
    <xs:attributeGroup ref="axf:axf_annotation-border-style"/>
    <xs:attributeGroup ref="axf:axf_annotation-border-width"/>
    <xs:attributeGroup ref="axf:axf_annotation-color"/>
    <xs:attributeGroup ref="axf:axf_annotation-contents"/>
    <xs:attributeGroup ref="axf:axf_annotation-file-attachment"/>
    <xs:attributeGroup ref="axf:axf_annotation-flags"/>
    <xs:attributeGroup ref="axf:axf_annotation-font-family"/>
    <xs:attributeGroup ref="axf:axf_annotation-font-size"/>
    <xs:attributeGroup ref="axf:axf_annotation-font-style"/>
    <xs:attributeGroup ref="axf:axf_annotation-font-weight"/>
    <xs:attributeGroup ref="axf:axf_annotation-height"/>
