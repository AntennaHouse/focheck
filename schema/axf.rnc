fo_basic-link.attlist &=
    axf_action-type,
    text-decoration

fo_block.attlist &=
    axf_destination-type,
    height,
    max-height,
    max-width,
    min-height,
    min-width,
    text-decoration,
    width,
    axf.block.attlist

fo_block-container.attlist &=
    axf_column-count,
    axf_column-gap,
    axf_destination-type,
    axf.block.attlist,
    column-count,
    role

fo_float.attlist &=
    axf_float,
    axf_float-x,
    axf_float-y,
    axf_float-reference,
    axf_float-move,
    axf_float-wrap,
    axf_float-min-wrap-x,
    axf_float-min-wrap-y,
    axf_float-centering-x,
    axf_float-centering-y,
    axf_float-margin-x,
    axf_float-margin-y,
    axf_float-float-margin-x,
    axf_float-float-margin-y,
    axf_float-offset-x,
    axf_float-offset-y

fo_footnote.attlist &=
    space-before,
    axf_footnote-align,
    axf_footnote-stacking,
    axf_footnote-position,
    axf_footnote-keep,
    axf_footnote-max-height

fo_instream-foreign-object.attlist &=
    axf_alttext

fo_list-block.attlist &=
    axf.block.attlist

fo_table.attlist &=
    axf.block.attlist

fo_table-and-caption.attlist &=
    axf.block.attlist

fo_table.attlist &=
    axf_table-summary

fo_table-cell.attlist &=
    axf_repeat-cell-content-at-break

fo_table-column.attlist &=
    id

fo_wrapper.attlist &=
    text-decoration

#
# Extensions to property groups
#

common-accessibility-properties &=
    axf_pdftag

common-border-padding-and-background-properties &=
    axf_background-content-width,
    axf_background-content-height,
    axf_background-content-type,
    axf_background-scaling,
    axf_border-radius,
    axf_border-top-left-radius,
    axf_border-top-right-radius,
    axf_border-bottom-left-radius,
    axf_border-bottom-right-radius


#
# Antenna House extension properties
#

# List of extension properties common to all block-level formatting
# objects
axf.block.attlist =
    axf_outline-expand,
    axf_outline-group,
    axf_outline-level,
    axf_outline-title,
    axf_outline-color,
    axf_outline-font-style,
    axf_outline-font-weight

# @axf:background-color
# @axf:background-image
# @axf:background-position-horizontal
# @axf:background-position-vertical
# @axf:background-repeat
#
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#IDAAOLV
axf_background-color =
    ## <color> | transparent
    attribute axf:background-color { text }?
axf_background-image =
    ## <uri-specification> | none
    attribute axf:background-image { text }?
axf_background-position-horizontal =
    ## <percentage> | <length> | left | center | right
    attribute axf:background-position-horizontal { text }?
axf_background-position-vertical =
    ## <percentage> | <length> | top | center | bottom
    attribute axf:background-position-vertical { text }?
axf_background-repeat =
    ## repeat | repeat-x | repeat-y | no-repeat | paginate
    attribute axf:background-repeat { text }?

# @axf:action-type
#
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf.action-type
axf_action-type =
    ## Specifies the action of external link or form action
    attribute axf:action-type { 'gotor' | 'launch' | 'uri' | 'reset' | 'submit' | 'auto' }?

# @axf:alt-text
#
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf.alttext
axf_alttext =
    ## alternate text of the image
    attribute axf:alttext { text }?

# @axf:background-content-height
# @axf:background-content-type
# @axf:background-content-width
# @axf:background-scaling
#
# http://www.antennahouse.com/product/ahf60/docs/ahf-ext.html#axf.background-content
axf_background-content-height =
    ## auto | scale-to-fit | scale-down-to-fit | scale-up-to-fit | <length> | <percentage> | inherit
    attribute axf:background-content-height { text }?
axf_background-content-type =
    ## <string> | auto
    attribute axf:background-content-type { text }?
axf_background-content-width =
    ## auto | scale-to-fit | scale-down-to-fit | scale-up-to-fit | <length> | <percentage> | inherit
    attribute axf:background-content-width { text }?
axf_background-scaling =
    ## uniform | non-uniform | inherit
    attribute axf:background-scaling { 'uniform' | 'non-uniform' | 'inherit' }?

