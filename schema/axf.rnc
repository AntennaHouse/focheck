default namespace axf = "http://www.antennahouse.com/names/XSL/Extensions"
namespace fo = "http://www.w3.org/1999/XSL/Format"
namespace local = ""

include "axf-inherited.rnc" {
#
# Overrides to patterns in 'fo-inherited.rnc' (and patterns in
# modules that it includes).
#

# http://www.antennahouse.com/product/ahf64/ahf-ext.html#fo.change-bar-begin
fo_change-bar-begin.model =
    ( fo_float? )

# Nested fo:page-sequence
# https://www.antennahouse.com/product/ahf64/ahf-ext.html#fo.page-sequence
fo_flow.model =
    fo_marker*,
    ( (block.fo.list | neutral.fo.list.block | fo_page-sequence)+
     & (point.fo.list)* )

fo_layout-master-set.model =
    ( (axf_spread-page-master|
       fo_simple-page-master|
       fo_page-sequence-master|
       fo_flow-map)+ )

# http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.document-info
fo_root.model =
    ( (axf_document-info*,
       fo_layout-master-set,
       axf_document-info*,
       fo_declarations?,
       axf_document-info*,
       fo_bookmark-tree?,
       axf_document-info*,
       (fo_page-sequence|fo_page-sequence-wrapper)+) )

# AH Formatter allows fo:region-* in any order.
fo_simple-page-master.model =
    ( (fo_region-body+ &
       fo_region-before? &
       fo_region-after? &
       fo_region-start? &
       fo_region-end?) )

# AH Formatter allows empty fo:static-content.
fo_static-content.model =
    ( ( (block.fo.list | neutral.fo.list)* ) & (point.fo.list)* )

# AH Formatter allows fo:block-container to omit block FOs.
fo_block-container.model =
    fo_marker*,
    ( ( (block.fo.list | neutral.fo.list)* ) & (point.fo.list)* )

# AH Formatter allows fo:inline-container to omit block FOs.
fo_inline-container.model =
    fo_marker*,
    ( ( (block.fo.list | neutral.fo.list)* ) & (point.fo.list)* )

# AH Formatter allows empty fo:list-item.
fo_list-item.model =
    fo_marker*,
    ( (fo_list-item-label,fo_list-item-body)? & (point.fo.list)* )

# AH Formatter allows fo:list-item-label omit block FOs.
fo_list-item-label.model =
    fo_marker*,
    ( ( (block.fo.list | neutral.fo.list)* ) & (point.fo.list)* )

# AH Formatter allows fo:table-cell to omit block FOs.
# axf:table-cell-repeated-marker used when cell repeats.
fo_table-cell.model =
    fo_marker*,
    axf_table-cell-repeated-marker?,
    ( ( (block.fo.list | neutral.fo.list)* ) & (point.fo.list)* )

# For fo:instream-foreign-object
# Exclude axf:* elements and attributes from counting as 'non-xsl'.
non-xsl =
  ( attribute * - ( local:* | axf:* | xml:* ) { text }*,
    element * - ( local:* | fo:* | axf:* ) { attribute * { text }*, anything }* )

#
# End of overrides to patterns in 'axf-inherited.rnc'.
#
}

#
# Antenna House extension elements
#

# <axf:document-info/>
# http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.document-info
axf_document-info =
  ## Document information such as an author and title that can be embedded in the PDF
  element axf:document-info {
    # 'name="title"' was used in V5.1 and still works but is
    # undocumented in V6.2.
    ## Name of the document information
    attribute name { ('document-title' | 'title' | 'subject' | 'author' | 'author-title' |
                       'description-writer' | 'keywords' | 'copyright-status' |
		       'copyright-notice' | 'copyright-info-url' | 'xmp' | 'pagemode' |
		       'pagelayout' | 'hidetoolbar' | 'hidemenubar' | 'hidewindowui' |
		       'fitwindow' | 'centerwindow' | 'displaydoctitle' | 'openaction' )},
    ## Value of the document information.  Some information has constraints on the allowed values.
    attribute value { text },
    empty
}

# <axf:footnote-number/>
# http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.footnote-number
axf_footnote-number =
  ## Generates a footnote number
  element axf:footnote-number {
    id,
    empty
}

# <axf:footnote-number-citation/>
# http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.footnote-number-citation
axf_footnote-number-citation =
  ## Cites a footnote number
  element axf:footnote-number-citation {
    attribute ref-id { text },
    empty
}

# <axf:form>
# https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.form
axf_form =
  element axf:form {
    axf_form.attlist,
    axf_form.model
}

axf_form.attlist =
  axf_field-submit-coordinates?,
  axf_field-submit-method?,
  external-destination?

axf_form.model =
  ( block.fo.list )*

# <axf:form-field>
#
# https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.form-field
axf_form-field =
  ## Generates a form field
  element axf:form-field {
    (axf_form-field-option | axf_form-field-event)*
}

# <axf:form-field-event>
#
# https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.form-field-event
axf_form-field-event =
  ## Defines the form action corresponding to an event
  element axf:form-field-event {
    text
}

# <axf:form-field-option>
#
# https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.form-field-option
axf_form-field-option =
  ## Specifies the component elements of the list box, combo box
  element axf:form-field-option {
    text
}

# <axf:ruby>
# https://www.antennahouse.com/product/ahf64/ahf-ruby.html#axf.ruby
axf_ruby =
  ## The ruby structure.
  element axf:ruby {
     axf.ruby.attlist,
     (axf_ruby-base, axf_ruby-text)+
}

# <axf:ruby-base>
# https://www.antennahouse.com/product/ahf64/ahf-ruby.html#axf.ruby-base
axf_ruby-base =
  ## Base character of ruby.
  element axf:ruby-base { text | axf_ruby }

# <axf:ruby-text>
# https://www.antennahouse.com/product/ahf64/ahf-ruby.html#axf.ruby-text
axf_ruby-text =
  ## Ruby.
  element axf:ruby-text { text }

# <axf:spread-page-master>
# http://www.antennahouse.com/product/ahf64/ahf-spread.html#axf.spread-page-master
axf_spread-page-master =
  ## A two-page spread.
  element axf:spread-page-master {
    axf_background-color,
    axf_background-content-height,
    axf_background-content-type,
    axf_background-content-width,
    axf_background-image,
    axf_background-position-horizontal,
    axf_background-position-vertical,
    axf_background-repeat,
    axf_background-scaling,
    axf_bleed,
    common-margin-properties-block,
    ## Master name of the page master for the left page of a two-page spread.
    attribute left-page-master-reference { xsd:NCName },
    master-name,
    reference-orientation,
    ## Master name of the page master for the right page of a two-page spread.
    attribute right-page-master-reference { xsd:NCName },
    writing-mode,
    background-attachment,
    background-image,
    background-position-horizontal,
    background-position-vertical,
    background-repeat,
    axf_spread-region+
}

# <axf:spread-region>
# http://www.antennahouse.com/product/ahf64/ahf-spread.html#axf.spread-region
axf_spread-region =
  ## Region in a two-page spread.
  element axf:spread-region {
    common-absolute-position-properties,
    common-border-padding-and-background-properties,
    common-margin-properties-block,
    height,
    region-name,
    reference-orientation,
    overflow,
    width,
    writing-mode,
    empty
}

# <axf:tab>
# https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.tab
axf_tab =
  ## Places a white space up to a tab stop position
  element axf:tab {
    axf_tab-align,
    empty
}

# <axf:table-cell-repeated-marker>
# http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.table-cell-repeated-marker
axf_table-cell-repeated-marker =
  ## Contents used when axf:repeat-cell-content-at-break="true"
  element axf:table-cell-repeated-marker {
    (text|inline.fo.list|block.fo.list)*
  }

