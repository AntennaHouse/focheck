  <define name="formatter-settings">
    <element name="formatter-settings">
      <optional>
        <attribute name="abbreviation-character-count">
          <data type="integer"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="auto-break-footnote">
          <a:documentation>Whether to automatically break footnotes when axf:footnote-max-height=â€‹"auto": true | false</a:documentation>
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="auto-formatter-type">
          <a:documentation>Formatter type when unclear if HTML or XHTML: html | xhtml</a:documentation>
          <choice>
            <value>html</value>
            <value>xhtml</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="avoid-orphan-hyphen"/>
      </optional>
      <optional>
        <attribute name="avoid-widow-words-CJK-punctuation">
          <a:documentation>Whether to count the last punctuation to be included in one character when axf:avoid-widow-word="true" is specified in CJK: true | false</a:documentation>
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="axf-formatter-config">
          <a:documentation>Whether &lt;axf:formatter-config&gt; is allowed</a:documentation>
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="baseline-mode">
          <a:documentation>Mode for handling baselines of mixed scripts: 4 = AH Formatter V4; 5 = AH Formatter V5; 6 = AH Formatter V6</a:documentation>
          <choice>
            <value>4</value>
            <value>5</value>
            <value>6</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="bidi-override-mode">
          <data type="integer"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="bold-ratio">
          <data type="decimal"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="border-medium-width">
          <data type="NMTOKEN"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="border-thick-width">
          <data type="NMTOKEN"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="border-thin-width">
          <data type="NMTOKEN"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="bpil">
          <a:documentation>Languages or scripts to which to apply Knuth-Plass Breaking Paragraphs into Lines [BPIL]</a:documentation>
        </attribute>
      </optional>
      <optional>
        <attribute name="bpil-limit-chars">
          <a:documentation>Maximum number of characters to which [BPIL] applies: &lt;integer&gt;</a:documentation>
          <data type="positiveInteger"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="bpil-minimum-line-width">
          <a:documentation>Minimum line width (in em) to which to apply [BPIL]: &lt;number&gt;</a:documentation>
        </attribute>
      </optional>
      <optional>
        <attribute name="bpil-penalty-hyphenation">
          <a:documentation>Frequency of hyphenation in [BPIL] with a value of 0 to 1000</a:documentation>
          <ref name="bpil-penalty-hyphenation.model"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="css-media-type">
          <data type="NCName"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="default-CJK">
          <data type="NCName"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="default-color"/>
      </optional>
      <optional>
        <attribute name="default-font-size">
          <data type="NMTOKEN"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="default-from-page-master-region">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="default-html-charset">
          <data type="NCName"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="default-lang"/>
      </optional>
      <optional>
        <attribute name="default-lang2">
          <data type="NCName"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="default-page-height">
          <data type="NMTOKEN"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="default-page-margin-bottom"/>
      </optional>
      <optional>
        <attribute name="default-page-margin-left"/>
      </optional>
      <optional>
        <attribute name="default-page-margin-right"/>
      </optional>
      <optional>
        <attribute name="default-page-margin-top"/>
      </optional>
      <optional>
        <attribute name="default-page-width">
          <data type="NMTOKEN"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="descendant-or-self-files">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="display-alttext">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="external-entity">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="family-name-syntax">
          <a:documentation>How strictly to check 'font-family' syntax: auto | loose | strict</a:documentation>
          <choice>
            <value>auto</value>
            <value>loose</value>
            <value>strict</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="fixed-width-space-treatment">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="hair-space-width"/>
      </optional>
      <optional>
        <attribute name="hyphenation-min">
          <a:documentation>Whether to enable &lt;hyphen-min&gt; in the Hyphenation Exception Dictionary: false | true | without-exceptions</a:documentation>
          <choice>
            <ref name="ah.boolean"/>
            <value>without-exceptions</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="HyphenationOption">
          <a:documentation>Whether to use custom hyphenation processing or TeX hyphenation dictionaries: 'true' | 'false'</a:documentation>
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="hyphenation-TeX">
          <a:documentation>Comma-delimited list of languages to hyphenate when HyphenationOption="true"</a:documentation>
        </attribute>
      </optional>
      <optional>
        <attribute name="hyphen-min">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="intrusion-displace-mode">
          <data type="integer"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="img-alt">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="issue-scale-to-fit">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="jamo-ligature">
          <a:documentation>Whether to process the ligature of Hangul Jamo (U+1100 to U+11FF)</a:documentation>
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="justify-leader">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="justify-rowspan-height">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="keep-footnote-anchor">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="latin-ligature">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="non-starter-ideographic-space">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="normal-line-height"/>
      </optional>
      <optional>
        <attribute name="normalize">
          <data type="NCName"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="oblique-skew">
          <data type="integer"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="omit-uppercase-word-accents"/>
      </optional>
      <optional>
        <attribute name="output-area-id">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="output-fo-tree">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="overflow-limit-block">
          <data type="NMTOKEN"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="overflow-limit-inline">
          <data type="NMTOKEN"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="pair-kerning">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="printer-marks-line-length"/>
      </optional>
      <optional>
        <attribute name="printer-marks-line-width"/>
      </optional>
      <optional>
        <attribute name="printer-marks-zero-margin"/>
      </optional>
      <optional>
        <attribute name="PrinterOrientation">
          <data type="NCName"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="PscriptPassThrough">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="punctuation-spacing"/>
      </optional>
      <optional>
        <attribute name="punctuation-trim">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="pxpi">
          <data type="integer"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="ruby-align"/>
      </optional>
      <optional>
        <attribute name="small-caps-emulation-size"/>
      </optional>
      <optional>
        <attribute name="SeparatePrinterDuplexJob">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="splitting-blocks-space">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="SVGViewer">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="tab-overlap-treatment">
          <choice>
            <value/>
            <value>ignore-tab</value>
            <value>next-tab</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="table-auto-layout-limit">
          <data type="integer"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="table-is-reference-area">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="text-autospace">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="text-autospace-width"/>
      </optional>
      <optional>
        <attribute name="text-decoration-baseline-mode">
          <data type="integer"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="text-decoration-mode">
          <data type="integer"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="text-justify-mode">
          <data type="integer"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="text-kashida-space"/>
      </optional>
      <optional>
        <attribute name="text-orientation-mode">
          <data type="integer"/>
        </attribute>
      </optional>
      <optional>
