  <!-- default namespace = "" -->
  <define name="ah.boolean">
    <choice>
      <value>false</value>
      <value>true</value>
    </choice>
  </define>
  <define name="bpil-penalty-hyphenation.model">
    <data type="integer">
      <param name="minInclusive">0</param>
      <param name="maxInclusive">1000</param>
    </data>
  </define>
  <define name="compression.list">
    <choice>
      <value>auto</value>
      <value>zlib</value>
      <value>jpeg</value>
      <value>jpeg2000</value>
      <value>keeplzw</value>
      <value>auto2k</value>
      <value>keeplzw2k</value>
    </choice>
  </define>
  <define name="downsampling.list">
    <choice>
      <value>none</value>
      <value>average</value>
      <value>bicubic</value>
      <value>subsampling</value>
    </choice>
  </define>
  <define name="dpi.model">
    <data type="positiveInteger">
      <param name="minInclusive">1</param>
    </data>
  </define>
  <define name="jpeg-quality.model">
    <data type="positiveInteger">
      <param name="minInclusive">1</param>
      <param name="maxInclusive">100</param>
    </data>
  </define>
  <define name="monochrome.compression.list">
    <choice>
      <value>ccitt4</value>
      <value>ccitt3</value>
      <value>runlength</value>
      <value>zlib</value>
      <value>none</value>
    </choice>
  </define>
  <define name="pass-through.model">
    <choice>
      <list>
        <oneOrMore>
          <choice>
            <value>all</value>
            <value>gif</value>
            <value>-gif</value>
            <value>tiff</value>
            <value>-tiff</value>
            <value>png</value>
            <value>-png</value>
            <value>jpeg</value>
            <value>-jpeg</value>
            <value>jpeg2000</value>
            <value>-jpeg2000</value>
            <value>jbig2</value>
            <value>-jbig2</value>
          </choice>
        </oneOrMore>
      </list>
      <value>none</value>
    </choice>
  </define>
