  </define>
  <define name="font-settings">
    <element name="font-settings">
      <a:documentation>Settings for fonts</a:documentation>
      <optional>
        <attribute name="auto-fallback-font">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="barcode-text-font">
          <a:documentation>Font for a linear barcode</a:documentation>
          <choice>
            <value>OCRB</value>
            <value>monospace</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="default-font-family">
          <a:documentation>Default font family</a:documentation>
          <data type="NCName"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="fallback-glyph">
          <a:documentation>Whether, and at what level, to report when a glyph is not found in a fallback font</a:documentation>
          <choice>
            <value>0</value>
            <value>1</value>
            <value>2</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="font-selection-mode">
          <a:documentation>Font selection method: 5 = disregard 'font-selection-strategy'; 6 = act on 'font-selection-strategy="character-by-character"'</a:documentation>
          <choice>
            <value>5</value>
            <value>6</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="font-selection-strategy">
          <data type="NCName"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="font-stretch-mode">
          <data type="integer"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="missing-font">
          <a:documentation>Whether to warn when font not found: [ 0 = no message; 1 = level 1 error message; 2 = level 2 error message ]</a:documentation>
          <choice>
            <value>0</value>
            <value>1</value>
            <value>2</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="missing-glyph">
          <a:documentation>Whether to warn when glyph not found: [ 0 = no message; 1 = level 1 error message; 2 = level 2 error message ]</a:documentation>
          <choice>
            <value>0</value>
            <value>1</value>
            <value>2</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="missing-glyph-all">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="use-windows-api">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <zeroOrMore>
        <ref name="script-font">
          <a:documentation>Mapping from generic font to actual font for a script</a:documentation>
        </ref>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="font-alias">
          <a:documentation>Alias one font name to another</a:documentation>
        </ref>
      </zeroOrMore>
    </element>
  </define>
  <define name="script-font">
    <element name="script-font">
      <ref name="script-font.attlist"/>
      <ref name="script-font.model"/>
    </element>
  </define>
  <define name="script-font.model">
    <empty/>
  </define>
  <define name="script-font.attlist">
    <optional>
      <attribute name="cursive">
        <a:documentation>Generic cursive font for script</a:documentation>
      </attribute>
    </optional>
    <optional>
      <attribute name="fallback">
        <a:documentation>Generic fallback font for script</a:documentation>
      </attribute>
    </optional>
    <optional>
      <attribute name="fantasy">
        <a:documentation>Generic fantasy font for script</a:documentation>
      </attribute>
    </optional>
