      <optional>
        <attribute name="text-underline-mode">
          <data type="integer"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="thin-space-width"/>
      </optional>
      <optional>
        <attribute name="two-pass-formatting">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="use-default-page-margin-CSS">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="use-default-page-margin-XSL">
          <ref name="ah.boolean"/>
        </attribute>
      </optional>
