  </define>
  <define name="script.list">
    <choice>
      <value>Latn</value>
      <value>Grek</value>
      <value>Cyrl</value>
      <value>Armn</value>
      <value>Geor</value>
      <value>Ethi</value>
      <value>Arab</value>
      <value>Syrc</value>
      <value>Hebr</value>
      <value>Deva</value>
      <value>Beng</value>
      <value>Guru</value>
      <value>Gujr</value>
      <value>Orya</value>
      <value>Taml</value>
      <value>Telu</value>
      <value>Knda</value>
      <value>Mlym</value>
      <value>Sinh</value>
      <value>Thai</value>
      <value>Khmr</value>
      <value>Laoo</value>
      <value>Mymr</value>
      <value>Kana</value>
      <value>Hira</value>
      <value>Hang</value>
      <value>Hani</value>
      <value>Hans</value>
      <value>Hant</value>
      <value>Jpan</value>
      <value>.</value>
      <value/>
    </choice>
  </define>
  <define name="language-code-space.atts">
    <attribute name="language">
      <a:documentation>Language for which to apply the space</a:documentation>
    </attribute>
    <optional>
      <attribute name="code">
        <a:documentation>Character code for which to apply the space</a:documentation>
      </attribute>
    </optional>
    <attribute name="space">
      <a:documentation>Amount of space to apply</a:documentation>
      <data type="decimal"/>
    </attribute>
  </define>
  <!-- start = formatter-config -->
  <define name="formatter-config">
    <element name="formatter-config">
      <ref name="formatter-config.model"/>
    </element>
  </define>
  <define name="formatter-config.model">
    <optional>
      <ref name="formatter-settings"/>
    </optional>
    <optional>
      <ref name="font-settings"/>
    </optional>
    <optional>
      <ref name="pdf-settings"/>
    </optional>
    <optional>
      <ref name="ps-settings"/>
    </optional>
    <optional>
      <ref name="svg-render-settings"/>
    </optional>
    <optional>
      <ref name="svg-settings"/>
    </optional>
    <optional>
      <ref name="text-settings"/>
    </optional>
    <optional>
      <ref name="mathml-settings"/>
    </optional>
    <optional>
      <ref name="cgm-settings"/>
    </optional>
    <optional>
      <ref name="xslt-settings"/>
    </optional>
    <optional>
      <ref name="analyzer-settings"/>
    </optional>
  </define>
  <define name="formatter-settings">
    <element name="formatter-settings">
      <ref name="formatter-settings.attlist"/>
      <ref name="formatter-settings.model"/>
    </element>
  </define>
  <define name="formatter-settings.model">
    <zeroOrMore>
      <ref name="list-style-type"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="quotationmark"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="script-chars"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="script-language-in-CJK"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="space-end-punctuation"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="space-start-punctuation"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="space-between-digit-and-punctuation"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="usercss"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="multimedia"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="GS1-128"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="UAX50"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="unbreakable-words"/>
    </zeroOrMore>
  </define>
  <define name="formatter-settings.attlist">
    <optional>
      <attribute name="abbreviation-character-count">
        <data type="integer"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="auto-break-footnote">
        <a:documentation>Whether to automatically break footnotes when axf:footnote-max-height=​"auto": true | false</a:documentation>
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="auto-formatter-type">
        <a:documentation>Formatter type when unclear if HTML or XHTML: html | xhtml</a:documentation>
        <choice>
          <value>html</value>
          <value>xhtml</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="avoid-widow-words-cjk-punctuation">
        <a:documentation>Whether to count the last punctuation to be included in one character when axf:avoid-widow-word="true" is specified in CJK: true | false</a:documentation>
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="axf-formatter-config">
        <a:documentation>Whether &lt;axf:formatter-config&gt; is allowed</a:documentation>
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="baseline-mode">
        <a:documentation>Mode for handling baselines of mixed scripts: 4 = XSL Formatter V4; 5 = Antenna House Formatter V5; 6 = Antenna House Formatter V6</a:documentation>
        <choice>
          <value>4</value>
          <value>5</value>
          <value>6</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="bidi-override-mode">
        <data type="integer"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="bold-ratio">
        <data type="decimal"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="border-medium-width">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="border-thick-width">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="border-thin-width">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="bpil">
        <a:documentation>Languages or scripts to which to apply Knuth-Plass Breaking Paragraphs into Lines [BPIL]</a:documentation>
      </attribute>
    </optional>
    <optional>
      <attribute name="bpil-limit-chars">
        <a:documentation>Maximum number of characters to which [BPIL] applies: &lt;integer&gt;</a:documentation>
        <data type="positiveInteger"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="bpil-minimum-line-width">
        <a:documentation>Minimum line width (in em) to which to apply [BPIL]: &lt;number&gt;</a:documentation>
      </attribute>
    </optional>
    <optional>
      <attribute name="bpil-penalty-hyphenation">
        <a:documentation>Frequency of hyphenation in [BPIL] with a value of 0 to 1000</a:documentation>
        <ref name="bpil-penalty-hyphenation.model"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="condensed-text-align-last">
        <a:documentation>Initial value when 'axf:condensed-text-align-last="auto"': true | false | justify</a:documentation>
        <choice>
          <ref name="ah.boolean"/>
          <value>justify</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="css-allow-bare-expression">
        <a:documentation>Whether expressions not enclosed in 'calc()' are allowed: true | false</a:documentation>
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="css-child-match-root">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="css-child-match-root">
        <a:documentation>Whether pseudo-classes match root element: true | false</a:documentation>
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="css-float-in-float">
        <a:documentation>Whether nested floats are allowed in CSS: true | false</a:documentation>
        <data type="NCName"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="default-CJK">
        <data type="NCName"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="default-color"/>
    </optional>
    <optional>
      <attribute name="default-font-size">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="default-from-page-master-region">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="default-html-charset">
        <data type="NCName"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="default-lang"/>
    </optional>
    <optional>
      <attribute name="default-lang2">
        <data type="NCName"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="default-page-height">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="default-page-margin-bottom"/>
    </optional>
    <optional>
      <attribute name="default-page-margin-left"/>
    </optional>
    <optional>
      <attribute name="default-page-margin-right"/>
    </optional>
    <optional>
      <attribute name="default-page-margin-top"/>
    </optional>
    <optional>
      <attribute name="default-page-width">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="descendant-or-self-files">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="display-alttext">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="external-entity">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="family-name-syntax">
        <a:documentation>How strictly to check 'font-family' syntax: auto | loose | strict</a:documentation>
        <choice>
          <value>auto</value>
          <value>loose</value>
          <value>strict</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="fixed-width-space-treatment">
        <a:documentation>Whether to insert a substitute glyph when a font does not include a white-space glyph: false | true | always</a:documentation>
        <choice>
          <ref name="ah.boolean"/>
          <value>always</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="hair-space-width">
        <a:documentation>Width, in em, of HAIR SPACE (U+200A)</a:documentation>
      </attribute>
    </optional>
    <optional>
      <attribute name="hyphenation-min">
        <a:documentation>Whether to enable &lt;hyphen-min&gt; in the Hyphenation Exception Dictionary: false | true | without-exceptions</a:documentation>
        <choice>
          <ref name="ah.boolean"/>
          <value>without-exceptions</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="HyphenationOption">
        <a:documentation>Whether to use custom hyphenation processing or TeX hyphenation dictionaries: 'true' | 'false'</a:documentation>
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="hyphenation-TeX">
        <a:documentation>Comma-delimited list of languages to hyphenate when HyphenationOption="true"</a:documentation>
      </attribute>
    </optional>
    <optional>
      <attribute name="hyphen-min">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="ignore-certificate-error">
        <a:documentation>Whether to ignore certificate errors: true | false</a:documentation>
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="intrusion-displace-mode">
        <data type="integer"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="img-alt">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="issue-scale-to-fit">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="jamo-ligature">
        <a:documentation>Whether to process the ligature of Hangul Jamo (U+1100 to U+11FF)</a:documentation>
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="justify-leader">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="justify-rowspan-height">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="keep-footnote-anchor">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="latin-ligature">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="non-starter-ideographic-space">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="normal-line-height"/>
    </optional>
    <optional>
      <attribute name="normalize">
        <data type="NCName"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="oblique-skew">
        <a:documentation>Angle when emulating font-style="oblique" or "backslant": &lt;number&gt;</a:documentation>
        <data type="integer"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="omit-uppercase-word-accents"/>
    </optional>
    <optional>
      <attribute name="output-area-id">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="output-fo-tree">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="overflow-limit-block">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="overflow-limit-inline">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="pair-kerning">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="printer-marks-line-length"/>
    </optional>
    <optional>
      <attribute name="printer-marks-line-width"/>
    </optional>
    <optional>
      <attribute name="printer-marks-zero-margin"/>
    </optional>
    <optional>
      <attribute name="PrinterOrientation">
        <data type="NCName"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="PscriptPassThrough">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="punctuation-spacing"/>
    </optional>
    <optional>
      <attribute name="punctuation-trim">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="pxpi">
        <data type="integer"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="responsive-svg-size">
        <a:documentation>Whether the size matches the viewBox setting when the width and height of the root element are omitted in SVG: reference | viewBox</a:documentation>
        <choice>
          <value>reference</value>
          <value>viewBox</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="ruby-align">
        <a:documentation>Ruby alignment when axf:ruby-align="auto"</a:documentation>
      </attribute>
    </optional>
    <optional>
      <attribute name="SeparatePrinterDuplexJob">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="small-caps-emulation-always">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="small-caps-emulation-size">
        <a:documentation>Scale-down ratio when the font does not have small-caps</a:documentation>
      </attribute>
    </optional>
    <optional>
      <attribute name="small-caps-emulation-x-height">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="splitting-blocks-space">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="tab-overlap-treatment">
        <a:documentation>Behavior when tab alignment makes letters overlap: ignore-tab | next-tab</a:documentation>
        <choice>
          <value>ignore-tab</value>
          <value>next-tab</value>
          <value/>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="table-auto-layout-limit">
        <data type="integer"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="table-is-reference-area">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="text-autospace">
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="text-autospace-mode">
        <choice>
          <value>6</value>
          <value>7</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="text-autospace-width"/>
    </optional>
    <optional>
      <attribute name="text-decoration-baseline-mode">
        <data type="integer"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="text-decoration-mode">
        <data type="integer"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="text-justify-mode">
        <data type="integer"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="text-kashida-space"/>
    </optional>
    <optional>
      <attribute name="text-orientation-mode">
        <data type="integer"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="textshadow-blur-cannot-embed-font">
        <a:documentation>Whether to apply blur to a font that is not allowed to be embedded: false | true</a:documentation>
        <ref name="ah.boolean"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="textshadow-resolution-minimum-dpi">
        <a:documentation>Minimum resolution of the image when applying blur in text-shadow: &lt;positive-integer&gt;</a:documentation>
        <ref name="dpi.model"/>
      </attribute>
