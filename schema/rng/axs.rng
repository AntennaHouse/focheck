      <value>PDF/X-3:2002</value>
      <value>PDF/X-1a:2003</value>
      <value>PDF/X-2:2003</value>
      <value>PDF/X-3:2003</value>
      <value>PDF/X-4:2010</value>
      <value>PDF/A-1a:2005</value>
      <value>PDF/A-1b:2005</value>
      <value>PDF1.4/A-2a:2011</value>
      <value>PDF1.5/A-2a:2011</value>
      <value>PDF1.6/A-2a:2011</value>
      <value>PDF1.7/A-2a:2011</value>
      <value>PDF1.4/A-2b:2011</value>
      <value>PDF1.5/A-2b:2011</value>
      <value>PDF1.6/A-2b:2011</value>
      <value>PDF1.7/A-2b:2011</value>
      <value>PDF1.4/A-2u:2011</value>
      <value>PDF1.5/A-2u:2011</value>
      <value>PDF1.6/A-2u:2011</value>
      <value>PDF1.7/A-2u:2011</value>
      <value>PDF1.4/A-3a:2012</value>
      <value>PDF1.5/A-3a:2012</value>
      <value>PDF1.6/A-3a:2012</value>
      <value>PDF1.7/A-3a:2012</value>
      <value>PDF1.4/A-3b:2012</value>
      <value>PDF1.5/A-3b:2012</value>
      <value>PDF1.6/A-3b:2012</value>
      <value>PDF1.7/A-3b:2012</value>
      <value>PDF1.4/A-3u:2012</value>
      <value>PDF1.5/A-3u:2012</value>
      <value>PDF1.6/A-3u:2012</value>
      <value>PDF1.7/A-3u:2012</value>
      <value>PDF1.5/UA-1:2014</value>
      <value>PDF1.6/UA-1:2014</value>
      <value>PDF1.7/UA-1:2014</value>
