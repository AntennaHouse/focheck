  <!-- start = formatter-config -->
  <define name="formatter-config">
    <element name="formatter-config">
      <ref name="formatter-config.model"/>
    </element>
  </define>
  <define name="formatter-config.model">
    <optional>
      <ref name="formatter-settings"/>
    </optional>
    <optional>
      <ref name="font-settings"/>
    </optional>
    <optional>
      <ref name="pdf-settings"/>
    </optional>
    <optional>
      <ref name="ps-settings"/>
    </optional>
    <optional>
      <ref name="mathml-settings"/>
    </optional>
    <optional>
      <ref name="rxe-settings"/>
