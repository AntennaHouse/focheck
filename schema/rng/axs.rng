          <ref name="ah.boolean"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="textshadow-resolution-minimum-dpi">
          <a:documentation>Minimum resolution of the image when applying blur in text-shadow: &lt;positive-integer&gt;</a:documentation>
          <ref name="dpi.model"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="textshadow-resolution-pixel-per-em">
