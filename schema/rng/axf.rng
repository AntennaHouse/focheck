<grammar xmlns:axh="http://www.antennahouse.com/names/XSL/Hyphenations" ns="" xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns:svg="http://www.w3.org/2000/svg" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:axs="http://www.antennahouse.com/names/XSL/Settings" xmlns:m="http://www.w3.org/1998/Math/MathML" xmlns:axf="http://www.antennahouse.com/names/XSL/Extensions" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <include href="axh.rng" ns="http://www.antennahouse.com/names/XSL/Extensions">
    <a:documentation>Hyphenation settings</a:documentation>
  </include>
  <include href="axs.rng" ns="http://www.antennahouse.com/names/XSL/Settings">
    <a:documentation>Option settings</a:documentation>
  </include>
  <include href="axf-inherited.rng" ns="http://www.antennahouse.com/names/XSL/Extensions">
    <!--
      
      Overrides to patterns in 'axf-inherited.rnc' (and patterns in
      modules that it includes).
      
    -->
    <!-- https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#fo.change-bar-begin -->
    <define name="fo_change-bar-begin.model">
      <optional>
        <ref name="fo_float"/>
      </optional>
    </define>
    <!--
      Nested fo:page-sequence
      https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#fo.page-sequence
    -->
    <define name="fo_flow.model">
      <zeroOrMore>
        <ref name="fo_marker"/>
      </zeroOrMore>
      <interleave>
        <oneOrMore>
          <choice>
            <ref name="block.fo.list"/>
            <ref name="neutral.fo.list.block"/>
            <ref name="fo_page-sequence"/>
          </choice>
        </oneOrMore>
        <zeroOrMore>
          <ref name="point.fo.list"/>
        </zeroOrMore>
      </interleave>
    </define>
    <define name="fo_layout-master-set.model">
      <oneOrMore>
        <choice>
          <ref name="axf_spread-page-master"/>
          <ref name="fo_simple-page-master"/>
          <ref name="fo_page-sequence-master"/>
          <ref name="fo_flow-map"/>
        </choice>
      </oneOrMore>
    </define>
    <!-- https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.document-info -->
    <define name="fo_root.model">
      <zeroOrMore>
        <ref name="axf_document-info"/>
      </zeroOrMore>
      <ref name="fo_layout-master-set"/>
      <zeroOrMore>
        <ref name="axf_document-info"/>
      </zeroOrMore>
      <optional>
        <ref name="fo_declarations"/>
      </optional>
      <zeroOrMore>
        <ref name="axf_document-info"/>
      </zeroOrMore>
      <optional>
        <ref name="fo_bookmark-tree"/>
      </optional>
      <zeroOrMore>
        <ref name="axf_document-info"/>
      </zeroOrMore>
      <oneOrMore>
        <choice>
          <ref name="fo_page-sequence"/>
          <ref name="fo_page-sequence-wrapper"/>
        </choice>
      </oneOrMore>
    </define>
    <!-- AH Formatter allows fo:region-* in any order. -->
    <define name="fo_simple-page-master.model">
      <interleave>
        <oneOrMore>
          <ref name="fo_region-body"/>
        </oneOrMore>
        <optional>
          <ref name="fo_region-before"/>
        </optional>
        <optional>
          <ref name="fo_region-after"/>
        </optional>
        <optional>
          <ref name="fo_region-start"/>
        </optional>
        <optional>
          <ref name="fo_region-end"/>
        </optional>
      </interleave>
    </define>
    <!-- AH Formatter uses some axf:* elements within fo:declarations. -->
    <define name="fo_declarations.model">
      <zeroOrMore>
        <choice>
          <ref name="fo_color-profile"/>
          <ref name="axf_custom-property"/>
          <ref name="axf_document-info"/>
          <ref name="axf_font-face"/>
          <ref name="axf_formatter-config"/>
          <ref name="axf_hyphenation-info"/>
          <ref name="axf_space-between-digit-and-punctuation"/>
          <ref name="axf_space-between-punctuation-and-digit"/>
          <ref name="axf_space-end-punctuation"/>
          <ref name="axf_space-start-punctuation"/>
          <ref name="axf_output-volume-info"/>
          <ref name="non-xsl-element"/>
        </choice>
      </zeroOrMore>
    </define>
    <!-- AH Formatter allows axf:document-info with multi-volume output. -->
    <define name="fo_page-sequence.model">
      <optional>
        <ref name="axf_document-info"/>
      </optional>
      <optional>
        <ref name="fo_title"/>
      </optional>
      <optional>
        <ref name="fo_folio-prefix"/>
      </optional>
      <optional>
        <ref name="fo_folio-suffix"/>
      </optional>
      <zeroOrMore>
        <ref name="fo_static-content"/>
      </zeroOrMore>
      <oneOrMore>
        <ref name="fo_flow"/>
      </oneOrMore>
    </define>
    <!-- AH Formatter allows empty fo:static-content. -->
    <define name="fo_static-content.model">
      <interleave>
        <zeroOrMore>
          <choice>
            <ref name="block.fo.list"/>
            <ref name="neutral.fo.list"/>
          </choice>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="point.fo.list"/>
        </zeroOrMore>
      </interleave>
    </define>
    <!-- AH Formatter allows fo:block-container to omit block FOs. -->
    <define name="fo_block-container.model">
      <zeroOrMore>
        <ref name="fo_marker"/>
      </zeroOrMore>
      <interleave>
        <zeroOrMore>
          <choice>
            <ref name="block.fo.list"/>
            <ref name="neutral.fo.list"/>
          </choice>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="point.fo.list"/>
        </zeroOrMore>
      </interleave>
    </define>
    <!-- AH Formatter allows fo:inline-container to omit block FOs. -->
    <define name="fo_inline-container.model">
      <zeroOrMore>
        <ref name="fo_marker"/>
      </zeroOrMore>
      <interleave>
        <zeroOrMore>
          <choice>
            <ref name="block.fo.list"/>
            <ref name="neutral.fo.list"/>
          </choice>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="point.fo.list"/>
        </zeroOrMore>
      </interleave>
    </define>
    <!-- AH Formatter allows empty fo:list-item. -->
    <define name="fo_list-item.model">
      <zeroOrMore>
        <ref name="fo_marker"/>
      </zeroOrMore>
      <interleave>
        <optional>
          <ref name="fo_list-item-label"/>
          <ref name="fo_list-item-body"/>
        </optional>
        <zeroOrMore>
          <ref name="point.fo.list"/>
        </zeroOrMore>
      </interleave>
    </define>
    <!-- AH Formatter allows fo:list-item-label omit block FOs. -->
    <define name="fo_list-item-label.model">
      <zeroOrMore>
        <ref name="fo_marker"/>
      </zeroOrMore>
      <interleave>
        <zeroOrMore>
          <choice>
            <ref name="block.fo.list"/>
            <ref name="neutral.fo.list"/>
          </choice>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="point.fo.list"/>
        </zeroOrMore>
      </interleave>
    </define>
    <!--
      AH Formatter allows fo:table-cell to omit block FOs.
      axf:table-cell-repeated-marker used when cell repeats.
    -->
    <define name="fo_table-cell.model">
      <zeroOrMore>
        <ref name="fo_marker"/>
      </zeroOrMore>
      <optional>
        <ref name="axf_table-cell-repeated-marker"/>
      </optional>
      <interleave>
        <zeroOrMore>
          <choice>
            <ref name="block.fo.list"/>
            <ref name="neutral.fo.list"/>
          </choice>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="point.fo.list"/>
        </zeroOrMore>
      </interleave>
    </define>
    <!--
      For fo:instream-foreign-object
      Exclude axf:* elements and attributes from counting as 'non-xsl'.
    -->
    <define name="non-xsl">
      <zeroOrMore>
        <attribute>
          <anyName>
            <except>
              <nsName ns=""/>
              <nsName/>
              <nsName ns="http://www.w3.org/XML/1998/namespace"/>
            </except>
          </anyName>
        </attribute>
      </zeroOrMore>
      <zeroOrMore>
        <element>
          <anyName>
            <except>
              <nsName ns=""/>
              <nsName ns="http://www.w3.org/1999/XSL/Format"/>
              <nsName/>
              <nsName ns="http://www.w3.org/1998/Math/MathML"/>
              <nsName ns="http://www.w3.org/2000/svg"/>
            </except>
          </anyName>
          <zeroOrMore>
            <attribute>
              <anyName/>
            </attribute>
          </zeroOrMore>
          <ref name="anything"/>
        </element>
      </zeroOrMore>
    </define>
    <!--
      For fo:declarations
      Exclude axf:* elements from counting as 'non-xsl'.
    -->
    <define name="non-xsl-element">
      <zeroOrMore>
        <element>
          <anyName>
            <except>
              <nsName ns=""/>
              <nsName ns="http://www.w3.org/1999/XSL/Format"/>
              <nsName/>
            </except>
          </anyName>
          <zeroOrMore>
            <attribute>
              <anyName>
                <except>
                  <name ns="">id</name>
                  <name ns="">ref-id</name>
                  <name ns="">internal-destination</name>
                </except>
              </anyName>
            </attribute>
          </zeroOrMore>
          <ref name="anything"/>
        </element>
      </zeroOrMore>
    </define>
    <!--
      
      End of overrides to patterns in 'axf-inherited.rnc'.
      
    -->
  </include>
  <!--
    
    Antenna House extension elements
    
  -->
  <!--
    <axf:custom-property/>
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.custom-property
  -->
  <define name="axf_custom-property">
    <element name="axf:custom-property">
      <a:documentation>Custom PDF property Effective with PDF 1.4 or later. Invalid with PDF/A</a:documentation>
      <attribute name="name">
        <a:documentation>Name of the custom property.  Should not be empty.</a:documentation>
      </attribute>
      <attribute name="value">
        <a:documentation>Value of the custom property.  Should not be empty.</a:documentation>
      </attribute>
      <empty/>
    </element>
  </define>
  <!--
    <axf:document-info/>
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.document-info
  -->
  <define name="axf_document-info">
    <element name="axf:document-info">
      <a:documentation>Document information such as an author and title that can be embedded in the PDF</a:documentation>
      <!--
        'name="title"' was used in V5.1 and still works but is
        undocumented in V6.2+.
      -->
      <attribute name="name">
        <a:documentation>Name of the document information</a:documentation>
        <choice>
          <value>document-title</value>
          <value>title</value>
          <value>subject</value>
          <value>author</value>
          <value>author-title</value>
          <value>description-writer</value>
          <value>keywords</value>
          <value>copyright-status</value>
          <value>copyright-notice</value>
          <value>copyright-info-url</value>
          <value>xmp</value>
          <value>pagemode</value>
          <value>pagelayout</value>
          <value>hidetoolbar</value>
          <value>hidemenubar</value>
          <value>hidewindowui</value>
          <value>fitwindow</value>
          <value>centerwindow</value>
          <value>displaydoctitle</value>
          <value>openaction</value>
        </choice>
      </attribute>
      <attribute name="value">
        <a:documentation>Value of the document information.  Some information has constraints on the allowed values.</a:documentation>
      </attribute>
      <empty/>
    </element>
  </define>
  <!--
    <axf:font-face>
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.font-face
  -->
  <define name="axf_font-face">
    <element name="axf:font-face">
      <a:documentation>Additional hyphenation information</a:documentation>
      <ref name="axf_font-face.attlist"/>
      <ref name="axf_font-face.model"/>
    </element>
  </define>
  <define name="axf_font-face.model">
    <empty/>
  </define>
  <define name="axf_font-face.attlist">
    <attribute name="src"/>
    <attribute name="font-family"/>
    <optional>
      <attribute name="font-style">
        <choice>
          <value>normal</value>
          <value>italic</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="font-weight">
        <choice>
          <value>normal</value>
          <value>bold</value>
          <value>100</value>
          <value>200</value>
          <value>300</value>
          <value>400</value>
          <value>500</value>
          <value>600</value>
          <value>700</value>
          <value>800</value>
          <value>900</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <!--
    <axf:footnote-number/>
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.footnote-number
  -->
  <define name="axf_footnote-number">
    <element name="axf:footnote-number">
      <a:documentation>Generates a footnote number</a:documentation>
      <ref name="id"/>
      <empty/>
    </element>
  </define>
  <!--
    <axf:footnote-number-citation/>
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.footnote-number-citation
  -->
  <define name="axf_footnote-number-citation">
    <element name="axf:footnote-number-citation">
      <a:documentation>Cites a footnote number</a:documentation>
      <attribute name="ref-id"/>
      <empty/>
    </element>
  </define>
  <!--
    <axf:form>
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.form
  -->
  <define name="axf_form">
    <element name="axf:form">
      <ref name="axf_form.attlist"/>
      <ref name="axf_form.model"/>
    </element>
  </define>
  <define name="axf_form.attlist">
    <optional>
      <ref name="axf_field-submit-coordinates"/>
    </optional>
    <optional>
      <ref name="axf_field-submit-method"/>
    </optional>
    <optional>
      <ref name="external-destination"/>
    </optional>
  </define>
  <define name="axf_form.model">
    <zeroOrMore>
      <ref name="block.fo.list"/>
    </zeroOrMore>
  </define>
  <!--
    <axf:form-field>
    
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.form-field
  -->
  <define name="axf_form-field">
    <element name="axf:form-field">
      <a:documentation>Generates a form field</a:documentation>
      <ref name="axf_form-field.attlist"/>
      <ref name="axf_form-field.model"/>
    </element>
  </define>
  <define name="axf_form-field.model">
    <zeroOrMore>
      <choice>
        <ref name="axf_form-field-option"/>
        <ref name="axf_form-field-event"/>
        <ref name="fo_external-graphic"/>
      </choice>
    </zeroOrMore>
  </define>
  <define name="axf_form-field.attlist">
    <!-- 'action-type' is required for <axf:form-field-event>. -->
    <ref name="axf_action-type.form"/>
    <ref name="axf_field-button-face"/>
    <ref name="axf_field-button-face-down"/>
    <ref name="axf_field-button-face-rollover"/>
    <ref name="axf_field-button-icon"/>
    <ref name="axf_field-button-icon-down"/>
    <ref name="axf_field-button-icon-rollover"/>
    <ref name="axf_field-button-layout"/>
    <ref name="axf_field-checked"/>
    <ref name="axf_field-checked-style"/>
    <ref name="axf_field-default-text"/>
    <ref name="axf_field-description"/>
    <ref name="axf_field-editable"/>
    <ref name="axf_field-flags"/>
    <ref name="axf_field-font-size"/>
    <ref name="axf_field-format"/>
    <ref name="axf_field-format-category"/>
    <ref name="axf_field-maxlen"/>
    <ref name="axf_field-multiline"/>
    <ref name="axf_field-multiple"/>
    <ref name="axf_field-name"/>
    <ref name="axf_field-name-suffix-page-number"/>
    <ref name="axf_field-scroll"/>
    <ref name="axf_field-text-align"/>
    <ref name="axf_field-top-index"/>
    <ref name="axf_field-type"/>
    <ref name="axf_field-value"/>
    <ref name="common-border-padding-and-background-properties"/>
    <ref name="external-destination"/>
    <ref name="height"/>
    <ref name="internal-destination"/>
    <ref name="width"/>
  </define>
  <!--
    <axf:form-field-event>
    
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.form-field-event
  -->
  <define name="axf_form-field-event">
    <element name="axf:form-field-event">
      <a:documentation>Defines the form action corresponding to an event</a:documentation>
      <ref name="axf_form-field-event.attlist"/>
      <text/>
    </element>
  </define>
  <define name="axf_form-field-event.attlist">
    <attribute>
      <a:documentation>Event type: MouseUp | MouseDown | MouseEnter | MouseExit | OnFocus | OnBlur</a:documentation>
      <choice>
        <name>axf:name</name>
        <name>name</name>
      </choice>
      <choice>
        <value>MouseUp</value>
        <value>MouseDown</value>
        <value>MouseEnter</value>
        <value>MouseExit</value>
        <value>OnFocus</value>
        <value>OnBlur</value>
      </choice>
    </attribute>
    <attribute>
      <a:documentation>Form action: auto | gotor | gotor | launch | uri | reset | submit | javascript</a:documentation>
      <choice>
        <name>axf:action-type</name>
        <name>action-type</name>
      </choice>
      <choice>
        <ref name="axf_action-type.form.model"/>
        <value>javascript</value>
      </choice>
    </attribute>
  </define>
  <!--
    <axf:form-field-option>
    
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.form-field-option
  -->
  <define name="axf_form-field-option">
    <element name="axf:form-field-option">
      <a:documentation>Specifies the component elements of the list box, combo box</a:documentation>
      <ref name="axf_form-field-option.attlist"/>
      <text/>
    </element>
  </define>
  <define name="axf_form-field-option.attlist">
    <ref name="axf_field-value"/>
  </define>
  <!--
    <axf:formatter-config>
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.formatter-config
  -->
  <define name="axf_formatter-config">
    <element name="axf:formatter-config">
      <a:documentation>Additional option setting information</a:documentation>
      <ref name="axf_formatter-config.attlist"/>
      <ref name="formatter-config.model"/>
    </element>
  </define>
  <!--
    axf_formatter-config.model =
      axs_pdf-settings?
  -->
  <define name="axf_formatter-config.attlist">
    <optional>
      <attribute name="src">
        <a:documentation>External Option Setting File</a:documentation>
      </attribute>
    </optional>
  </define>
  <!--
    <axf:hyphenation-info>
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.hyphenation-info
  -->
  <define name="axf_hyphenation-info">
    <element name="axf:hyphenation-info">
      <a:documentation>Additional hyphenation information</a:documentation>
      <ref name="axf_hyphenation-info.attlist"/>
      <optional>
        <ref name="axh_exceptions"/>
      </optional>
    </element>
  </define>
  <define name="axf_hyphenation-info.attlist">
    <optional>
      <attribute name="language"/>
    </optional>
    <optional>
      <attribute name="src"/>
    </optional>
  </define>
  <!--
    <axf:output-volume-info>
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ext.html#axf.output-volume-info
  -->
  <define name="axf_output-volume-info">
    <element name="axf:output-volume-info">
      <a:documentation>Output volume specification</a:documentation>
      <ref name="axf_output-volume-info.attlist"/>
      <empty/>
    </element>
  </define>
  <define name="axf_output-volume-info.attlist">
    <optional>
      <attribute name="initial-volume-number">
        <a:documentation>Initial volume number: &lt;positive-integer&gt;</a:documentation>
        <data type="positiveInteger"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="format">
        <a:documentation>Format specification: &lt;string&gt;</a:documentation>
      </attribute>
    </optional>
    <optional>
      <attribute name="bookmark-include">
        <a:documentation>How to include bookmarks in multiple separate volumes: first | all | separate | separate-group</a:documentation>
        <choice>
          <value>first</value>
          <value>all</value>
          <value>separate</value>
          <value>separate-group</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="document-info-include">
        <a:documentation>How to include document information in multiple separate volumes: first | all</a:documentation>
        <choice>
          <value>first</value>
          <value>all</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <!--
    <axf:ruby>
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ruby.html#axf.ruby
  -->
  <define name="axf_ruby">
    <element name="axf:ruby">
      <a:documentation>The ruby structure</a:documentation>
      <ref name="axf.ruby.attlist"/>
      <oneOrMore>
        <ref name="axf_ruby-base"/>
        <ref name="axf_ruby-text"/>
      </oneOrMore>
    </element>
  </define>
  <!--
    <axf:ruby-base>
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ruby.html#axf.ruby-base
  -->
  <define name="axf_ruby-base">
    <element name="axf:ruby-base">
      <a:documentation>Base character of ruby</a:documentation>
      <choice>
        <text/>
        <ref name="axf_ruby"/>
      </choice>
    </element>
  </define>
  <!--
    <axf:ruby-text>
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ruby.html#axf.ruby-text
  -->
  <define name="axf_ruby-text">
    <element name="axf:ruby-text">
      <a:documentation>Ruby</a:documentation>
      <text/>
    </element>
  </define>
  <!--
    <axf:space-end-punctuation>
    https://www.antenna.co.jp/AHF/help/v70e/ahf-ruby.html#axf.space-end-punctuation
  -->
  <define name="axf_space-end-punctuation">
    <element name="axf:space-end-punctuation">
      <a:documentation>Amount of space after punctuation</a:documentation>
