<grammar ns="http://www.antennahouse.com/names/XSL/Extensions" xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:axf="http://www.antennahouse.com/names/XSL/Extensions" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <include href="axf-inherited.rng">
    <!--
      
      Overrides to patterns in 'fo-inherited.rnc' (and patterns in
      modules that it includes).
      
    -->
    <!-- http://www.antennahouse.com/product/ahf64/ahf-ext.html#fo.change-bar-begin -->
    <define name="fo_change-bar-begin.model">
      <optional>
        <ref name="fo_float"/>
      </optional>
    </define>
    <!--
      Nested fo:page-sequence
      https://www.antennahouse.com/product/ahf64/ahf-ext.html#fo.page-sequence
    -->
    <define name="fo_flow.model">
      <zeroOrMore>
        <ref name="fo_marker"/>
      </zeroOrMore>
      <interleave>
        <oneOrMore>
          <choice>
            <ref name="block.fo.list"/>
            <ref name="neutral.fo.list.block"/>
            <ref name="fo_page-sequence"/>
          </choice>
        </oneOrMore>
        <zeroOrMore>
          <ref name="point.fo.list"/>
        </zeroOrMore>
      </interleave>
    </define>
    <define name="fo_layout-master-set.model">
      <oneOrMore>
        <choice>
          <ref name="axf_spread-page-master"/>
          <ref name="fo_simple-page-master"/>
          <ref name="fo_page-sequence-master"/>
          <ref name="fo_flow-map"/>
        </choice>
      </oneOrMore>
    </define>
    <!-- http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.document-info -->
    <define name="fo_root.model">
      <zeroOrMore>
        <ref name="axf_document-info"/>
      </zeroOrMore>
      <ref name="fo_layout-master-set"/>
      <zeroOrMore>
        <ref name="axf_document-info"/>
      </zeroOrMore>
      <optional>
        <ref name="fo_declarations"/>
      </optional>
      <zeroOrMore>
        <ref name="axf_document-info"/>
      </zeroOrMore>
      <optional>
        <ref name="fo_bookmark-tree"/>
      </optional>
      <zeroOrMore>
        <ref name="axf_document-info"/>
      </zeroOrMore>
      <oneOrMore>
        <choice>
          <ref name="fo_page-sequence"/>
          <ref name="fo_page-sequence-wrapper"/>
        </choice>
      </oneOrMore>
    </define>
    <!-- AH Formatter allows fo:region-* in any order. -->
    <define name="fo_simple-page-master.model">
      <interleave>
        <oneOrMore>
          <ref name="fo_region-body"/>
        </oneOrMore>
        <optional>
          <ref name="fo_region-before"/>
        </optional>
        <optional>
          <ref name="fo_region-after"/>
        </optional>
        <optional>
          <ref name="fo_region-start"/>
        </optional>
        <optional>
          <ref name="fo_region-end"/>
        </optional>
      </interleave>
    </define>
    <!-- AH Formatter allows empty fo:static-content. -->
    <define name="fo_static-content.model">
      <interleave>
        <zeroOrMore>
          <choice>
            <ref name="block.fo.list"/>
            <ref name="neutral.fo.list"/>
          </choice>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="point.fo.list"/>
        </zeroOrMore>
      </interleave>
    </define>
    <!-- AH Formatter allows fo:block-container to omit block FOs. -->
    <define name="fo_block-container.model">
      <zeroOrMore>
        <ref name="fo_marker"/>
      </zeroOrMore>
      <interleave>
        <zeroOrMore>
          <choice>
            <ref name="block.fo.list"/>
            <ref name="neutral.fo.list"/>
          </choice>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="point.fo.list"/>
        </zeroOrMore>
      </interleave>
    </define>
    <!-- AH Formatter allows fo:inline-container to omit block FOs. -->
    <define name="fo_inline-container.model">
      <zeroOrMore>
        <ref name="fo_marker"/>
      </zeroOrMore>
      <interleave>
        <zeroOrMore>
          <choice>
            <ref name="block.fo.list"/>
            <ref name="neutral.fo.list"/>
          </choice>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="point.fo.list"/>
        </zeroOrMore>
      </interleave>
    </define>
    <!-- AH Formatter allows empty fo:list-item. -->
    <define name="fo_list-item.model">
      <zeroOrMore>
        <ref name="fo_marker"/>
      </zeroOrMore>
      <interleave>
        <optional>
          <ref name="fo_list-item-label"/>
          <ref name="fo_list-item-body"/>
        </optional>
        <zeroOrMore>
          <ref name="point.fo.list"/>
        </zeroOrMore>
      </interleave>
    </define>
    <!-- AH Formatter allows fo:list-item-label omit block FOs. -->
    <define name="fo_list-item-label.model">
      <zeroOrMore>
        <ref name="fo_marker"/>
      </zeroOrMore>
      <interleave>
        <zeroOrMore>
          <choice>
            <ref name="block.fo.list"/>
            <ref name="neutral.fo.list"/>
          </choice>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="point.fo.list"/>
        </zeroOrMore>
      </interleave>
    </define>
    <!--
      AH Formatter allows fo:table-cell to omit block FOs.
      axf:table-cell-repeated-marker used when cell repeats.
    -->
    <define name="fo_table-cell.model">
      <zeroOrMore>
        <ref name="fo_marker"/>
      </zeroOrMore>
      <optional>
        <ref name="axf_table-cell-repeated-marker"/>
      </optional>
      <interleave>
        <zeroOrMore>
          <choice>
            <ref name="block.fo.list"/>
            <ref name="neutral.fo.list"/>
          </choice>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="point.fo.list"/>
        </zeroOrMore>
      </interleave>
    </define>
    <!--
      For fo:instream-foreign-object
      Exclude axf:* elements and attributes from counting as 'non-xsl'.
    -->
    <define name="non-xsl">
      <zeroOrMore>
        <attribute>
          <anyName>
            <except>
              <nsName ns=""/>
              <nsName/>
              <nsName ns="http://www.w3.org/XML/1998/namespace"/>
            </except>
          </anyName>
        </attribute>
      </zeroOrMore>
      <zeroOrMore>
        <element>
          <anyName>
            <except>
              <nsName ns=""/>
              <nsName ns="http://www.w3.org/1999/XSL/Format"/>
              <nsName/>
            </except>
          </anyName>
          <zeroOrMore>
            <attribute>
              <anyName/>
            </attribute>
          </zeroOrMore>
          <ref name="anything"/>
        </element>
      </zeroOrMore>
    </define>
    <!--
      
      End of overrides to patterns in 'axf-inherited.rnc'.
      
    -->
  </include>
  <!--
    
    Antenna House extension elements
    
  -->
  <!--
    <axf:document-info/>
    http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.document-info
  -->
  <define name="axf_document-info">
    <element name="axf:document-info">
      <a:documentation>Document information such as an author and title that can be embedded in the PDF</a:documentation>
      <!--
        'name="title"' was used in V5.1 and still works but is
        undocumented in V6.2.
      -->
      <attribute name="name">
        <a:documentation>Name of the document information</a:documentation>
        <choice>
          <value>document-title</value>
          <value>title</value>
          <value>subject</value>
          <value>author</value>
          <value>author-title</value>
          <value>description-writer</value>
          <value>keywords</value>
          <value>copyright-status</value>
          <value>copyright-notice</value>
          <value>copyright-info-url</value>
          <value>xmp</value>
          <value>pagemode</value>
          <value>pagelayout</value>
          <value>hidetoolbar</value>
          <value>hidemenubar</value>
          <value>hidewindowui</value>
          <value>fitwindow</value>
          <value>centerwindow</value>
          <value>displaydoctitle</value>
          <value>openaction</value>
        </choice>
      </attribute>
      <attribute name="value">
        <a:documentation>Value of the document information.  Some information has constraints on the allowed values.</a:documentation>
      </attribute>
      <empty/>
    </element>
  </define>
  <!--
    <axf:footnote-number/>
    http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.footnote-number
  -->
  <define name="axf_footnote-number">
    <element name="axf:footnote-number">
      <a:documentation>Generates a footnote number</a:documentation>
      <ref name="id"/>
      <empty/>
    </element>
  </define>
  <!--
    <axf:footnote-number-citation/>
    http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.footnote-number-citation
  -->
  <define name="axf_footnote-number-citation">
    <element name="axf:footnote-number-citation">
      <a:documentation>Cites a footnote number</a:documentation>
      <attribute name="ref-id"/>
      <empty/>
    </element>
  </define>
  <!--
    <axf:form>
    https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.form
  -->
  <define name="axf_form">
    <element name="axf:form">
      <ref name="axf_form.attlist"/>
      <ref name="axf_form.model"/>
    </element>
  </define>
  <define name="axf_form.attlist">
    <optional>
      <ref name="axf_field-submit-coordinates"/>
    </optional>
    <optional>
      <ref name="axf_field-submit-method"/>
    </optional>
    <optional>
      <ref name="external-destination"/>
    </optional>
  </define>
  <define name="axf_form.model">
    <zeroOrMore>
      <ref name="block.fo.list"/>
    </zeroOrMore>
  </define>
  <!--
    <axf:form-field>
    
    https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.form-field
  -->
  <define name="axf_form-field">
    <element name="axf:form-field">
      <a:documentation>Generates a form field</a:documentation>
      <zeroOrMore>
        <choice>
          <ref name="axf_form-field-option"/>
          <ref name="axf_form-field-event"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <!--
    <axf:form-field-event>
    
    https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.form-field-event
  -->
  <define name="axf_form-field-event">
    <element name="axf:form-field-event">
      <a:documentation>Defines the form action corresponding to an event</a:documentation>
      <text/>
    </element>
  </define>
  <!--
    <axf:form-field-option>
    
    https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.form-field-option
  -->
  <define name="axf_form-field-option">
    <element name="axf:form-field-option">
      <a:documentation>Specifies the component elements of the list box, combo box</a:documentation>
      <text/>
    </element>
  </define>
  <!--
    <axf:ruby>
    https://www.antennahouse.com/product/ahf64/ahf-ruby.html#axf.ruby
  -->
  <define name="axf_ruby">
    <element name="axf:ruby">
      <a:documentation>The ruby structure.</a:documentation>
      <ref name="axf.ruby.attlist"/>
      <oneOrMore>
        <ref name="axf_ruby-base"/>
        <ref name="axf_ruby-text"/>
      </oneOrMore>
    </element>
  </define>
  <!--
    <axf:ruby-base>
    https://www.antennahouse.com/product/ahf64/ahf-ruby.html#axf.ruby-base
  -->
  <define name="axf_ruby-base">
    <element name="axf:ruby-base">
      <a:documentation>Base character of ruby.</a:documentation>
      <choice>
        <text/>
        <ref name="axf_ruby"/>
      </choice>
    </element>
  </define>
  <!--
    <axf:ruby-text>
    https://www.antennahouse.com/product/ahf64/ahf-ruby.html#axf.ruby-text
  -->
  <define name="axf_ruby-text">
    <element name="axf:ruby-text">
      <a:documentation>Ruby.</a:documentation>
      <text/>
    </element>
  </define>
  <!--
    <axf:spread-page-master>
    http://www.antennahouse.com/product/ahf64/ahf-spread.html#axf.spread-page-master
  -->
  <define name="axf_spread-page-master">
    <element name="axf:spread-page-master">
      <a:documentation>A two-page spread.</a:documentation>
      <ref name="axf_background-color"/>
      <ref name="axf_background-content-height"/>
      <ref name="axf_background-content-type"/>
      <ref name="axf_background-content-width"/>
      <ref name="axf_background-image"/>
      <ref name="axf_background-position-horizontal"/>
      <ref name="axf_background-position-vertical"/>
      <ref name="axf_background-repeat"/>
      <ref name="axf_background-scaling"/>
      <ref name="axf_bleed"/>
      <ref name="common-margin-properties-block"/>
      <attribute name="left-page-master-reference">
        <a:documentation>Master name of the page master for the left page of a two-page spread.</a:documentation>
        <data type="NCName"/>
      </attribute>
      <ref name="master-name"/>
      <ref name="reference-orientation"/>
      <attribute name="right-page-master-reference">
        <a:documentation>Master name of the page master for the right page of a two-page spread.</a:documentation>
        <data type="NCName"/>
      </attribute>
      <ref name="writing-mode"/>
      <ref name="background-attachment"/>
      <ref name="background-image"/>
      <ref name="background-position-horizontal"/>
      <ref name="background-position-vertical"/>
      <ref name="background-repeat"/>
      <oneOrMore>
        <ref name="axf_spread-region"/>
      </oneOrMore>
    </element>
  </define>
  <!--
    <axf:spread-region>
    http://www.antennahouse.com/product/ahf64/ahf-spread.html#axf.spread-region
  -->
  <define name="axf_spread-region">
    <element name="axf:spread-region">
      <a:documentation>Region in a two-page spread.</a:documentation>
      <ref name="common-absolute-position-properties"/>
      <ref name="common-border-padding-and-background-properties"/>
      <ref name="common-margin-properties-block"/>
      <ref name="height"/>
      <ref name="region-name"/>
      <ref name="reference-orientation"/>
      <ref name="overflow"/>
      <ref name="width"/>
      <ref name="writing-mode"/>
      <empty/>
    </element>
  </define>
  <!--
    <axf:tab>
    https://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.tab
  -->
  <define name="axf_tab">
    <element name="axf:tab">
      <a:documentation>Places a white space up to a tab stop position</a:documentation>
      <ref name="axf_tab-align"/>
      <empty/>
    </element>
  </define>
  <!--
    <axf:table-cell-repeated-marker>
    http://www.antennahouse.com/product/ahf64/ahf-ext.html#axf.table-cell-repeated-marker
  -->
  <define name="axf_table-cell-repeated-marker">
    <element name="axf:table-cell-repeated-marker">
      <a:documentation>Contents used when axf:repeat-cell-content-at-break="true"</a:documentation>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="inline.fo.list"/>
          <ref name="block.fo.list"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <!--
    
    Antenna House extensions to patterns in included modules.
    
  -->
  <!--
    
    Extensions to FO models
    
  -->
  <define name="block.fo.list" combine="choice">
    <ref name="axf_form"/>
  </define>
  <define name="inline.fo.list" combine="choice">
    <choice>
      <ref name="axf_footnote-number"/>
      <ref name="axf_footnote-number-citation"/>
      <ref name="axf_form-field"/>
      <ref name="axf_ruby"/>
      <ref name="axf_tab"/>
    </choice>
  </define>
  <!--
    
    Extensions to FO attlists
    
  -->
  <!-- FOs that are not descendants of fo:flow -->
  <define name="fo_root.attlist" combine="interleave">
    <ref name="xml_id"/>
  </define>
  <define name="fo_page-sequence.attlist" combine="interleave">
    <ref name="axf_background-color"/>
    <ref name="axf_background-image"/>
    <ref name="axf_background-position-horizontal"/>
    <ref name="axf_background-position-vertical"/>
    <ref name="axf_background-repeat"/>
    <ref name="axf_output-volume-break"/>
    <ref name="axf_output-volume-filename"/>
    <ref name="axf_page-number-prefix"/>
    <ref name="axf_printer-bin-selection"/>
    <ref name="axf_printer-duplex"/>
    <ref name="background-color"/>
    <ref name="background-image"/>
    <ref name="background-position-horizontal"/>
    <ref name="background-position-vertical"/>
    <ref name="background-repeat"/>
    <ref name="xml_id"/>
  </define>
  <define name="fo_page-sequence-wrapper.attlist" combine="interleave">
    <ref name="xml_id"/>
  </define>
  <define name="fo_simple-page-master.attlist" combine="interleave">
    <ref name="axf_background-color"/>
    <ref name="axf_background-content-height"/>
    <ref name="axf_background-content-type"/>
    <ref name="axf_background-content-width"/>
    <ref name="axf_background-image"/>
    <ref name="axf_background-position-horizontal"/>
    <ref name="axf_background-position-vertical"/>
    <ref name="axf_background-repeat"/>
    <ref name="axf_background-scaling"/>
    <ref name="axf_bleed"/>
    <ref name="axf_bleed-top"/>
    <ref name="axf_bleed-bottom"/>
    <ref name="axf_bleed-left"/>
    <ref name="axf_bleed-right"/>
    <ref name="axf_crop-area-visibility"/>
    <ref name="axf_crop-offset"/>
    <ref name="axf_crop-offset-top"/>
    <ref name="axf_crop-offset-bottom"/>
    <ref name="axf_crop-offset-left"/>
    <ref name="axf_crop-offset-right"/>
    <ref name="axf_printer-marks"/>
    <ref name="axf_printer-marks-line-color"/>
    <ref name="axf_printer-marks-line-length"/>
    <ref name="axf_printer-marks-line-width"/>
    <ref name="axf_printer-marks-spine-width"/>
    <ref name="axf_printer-marks-zero-margin"/>
    <ref name="background-color"/>
    <ref name="background-image"/>
    <ref name="background-position-horizontal"/>
    <ref name="background-position-vertical"/>
    <ref name="background-repeat"/>
  </define>
  <define name="fo_region-body.attlist" combine="interleave">
    <ref name="axf_footnote-align"/>
    <ref name="axf_footnote-keep"/>
    <ref name="axf_footnote-max-height"/>
    <ref name="axf_footnote-position"/>
    <ref name="axf_footnote-stacking"/>
  </define>
  <define name="fo_flow.attlist" combine="interleave">
    <ref name="axf_baseline-grid"/>
    <ref name="xml_id"/>
  </define>
  <define name="fo_static-content.attlist" combine="interleave">
    <ref name="axf_baseline-grid"/>
    <ref name="xml_id"/>
  </define>
  <!-- FOs in flow listed in alphabetic order -->
  <define name="fo_basic-link.attlist" combine="interleave">
    <ref name="axf_action-type"/>
    <ref name="axf_alttext"/>
    <ref name="text-decoration"/>
    <ref name="xml_id"/>
    <ref name="axf.annotation.attlist"/>
  </define>
  <define name="fo_bidi-override.attlist" combine="interleave">
    <ref name="xml_id"/>
    <ref name="axf.annotation.attlist"/>
  </define>
  <define name="fo_block.attlist" combine="interleave">
    <ref name="axf_destination-type"/>
    <ref name="axf_suppress-if-first-on-page"/>
    <ref name="height"/>
    <ref name="max-height"/>
    <ref name="max-width"/>
    <ref name="min-height"/>
    <ref name="min-width"/>
    <ref name="text-decoration"/>
    <ref name="width"/>
    <ref name="xml_id"/>
    <ref name="axf.annotation.attlist"/>
    <ref name="axf.block.attlist"/>
  </define>
  <define name="fo_block-container.attlist" combine="interleave">
    <ref name="axf_column-count"/>
    <ref name="axf_column-gap"/>
    <ref name="axf_destination-type"/>
    <ref name="axf_suppress-if-first-on-page"/>
    <ref name="axf.block.attlist"/>
    <ref name="column-count"/>
    <ref name="column-gap"/>
    <ref name="role"/>
    <ref name="xml_id"/>
    <ref name="axf.annotation.attlist"/>
  </define>
  <define name="fo_character.attlist" combine="interleave">
    <ref name="xml_id"/>
    <ref name="axf.annotation.attlist"/>
  </define>
  <define name="fo_external-graphic.attlist" combine="interleave">
    <ref name="axf_alttext"/>
    <ref name="xml_id"/>
    <ref name="axf.annotation.attlist"/>
  </define>
  <define name="fo_float.attlist" combine="interleave">
    <ref name="axf_float"/>
    <ref name="axf_float-x"/>
    <ref name="axf_float-y"/>
    <ref name="axf_float-reference"/>
    <ref name="axf_float-move"/>
    <ref name="axf_float-wrap"/>
    <ref name="axf_float-min-wrap-x"/>
    <ref name="axf_float-min-wrap-y"/>
    <ref name="axf_float-centering-x"/>
    <ref name="axf_float-centering-y"/>
    <ref name="axf_float-margin-x"/>
    <ref name="axf_float-margin-y"/>
    <ref name="axf_float-float-margin-x"/>
    <ref name="axf_float-float-margin-y"/>
    <ref name="axf_float-offset-x"/>
    <ref name="axf_float-offset-y"/>
    <ref name="xml_id"/>
    <ref name="axf.annotation.attlist"/>
  </define>
  <define name="fo_footnote.attlist" combine="interleave">
    <ref name="space-before"/>
    <ref name="axf_footnote-align"/>
    <ref name="axf_footnote-stacking"/>
    <ref name="axf_footnote-position"/>
    <ref name="axf_footnote-keep"/>
    <ref name="xml_id"/>
    <ref name="axf.annotation.attlist"/>
  </define>
  <define name="fo_footnote-body.attlist" combine="interleave">
    <ref name="xml_id"/>
    <ref name="axf.annotation.attlist"/>
  </define>
  <define name="fo_index-range-begin.attlist" combine="interleave">
    <ref name="xml_id"/>
    <ref name="axf.annotation.attlist"/>
  </define>
  <define name="fo_inline.attlist" combine="interleave">
    <ref name="xml_id"/>
    <ref name="axf.annotation.attlist"/>
    <ref name="axf_indent-here"/>
  </define>
  <define name="fo_inline-container.attlist" combine="interleave">
    <ref name="xml_id"/>
    <ref name="axf.annotation.attlist"/>
  </define>
  <define name="fo_instream-foreign-object.attlist" combine="interleave">
    <ref name="axf_alttext"/>
    <ref name="xml_id"/>
    <ref name="axf.annotation.attlist"/>
