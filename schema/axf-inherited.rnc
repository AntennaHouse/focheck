#
# Properties
#

axf_abbreviation-character-count = 
    ## Minimum number of characters considered to be an abbreviation
    attribute axf:abbreviation-character-count { 'auto' | text }?
axf_alt-glyph = 
    ## Alternative glyph of a character: <number> | <string> <number>?
    attribute axf:alt-glyph { text }?
axf_assumed-page-number = 
    ## Assumed page number
    attribute axf:assumed-page-number { text }?
axf_avoid-widow-words = 
    ## Spacing behavior between words or characters so that the last line of the paragraph does not have only one word left (one character for CJK)
    attribute axf:avoid-widow-words { 'true' | 'false' }?
axf_base-uri = 
    ## Location which becomes the base of relative URI
    attribute axf:base-uri { text }?
axf_diagonal-border-color = 
    ## Color of the diagonal border
    attribute axf:diagonal-border-color { text }?
axf_diagonal-border-width = 
    ## Width of the diagonal border
    attribute axf:diagonal-border-width { text }?
axf_hanging-punctuation = 
    ## Whether to hang punctuations at the start of the line or end of the line: none | [ start || first || [ force-end | allow-end ] || last ]
    attribute axf:hanging-punctuation { text }?
axf_hyphenate-hyphenated-word = 
    ## Whether to hyphenate an already hyphenated word
    attribute axf:hyphenate-hyphenated-word { 'true' | 'false' }?
axf_hyphenation-minimum-character-count = 
    ## Minimum number of the character to hyphenate
    attribute axf:hyphenation-minimum-character-count { text }?
axf_hyphenation-zone = 
    ## Range where a hyphenation is available: none | <length>
    attribute axf:hyphenation-zone { 'none' | text }?
axf_japanese-glyph = 
    ## Glyph of Japanese Kanji
    attribute axf:japanese-glyph { 'none' | 'jp78' | 'jp83' | 'jp90' | 'jp04' }?
axf_justify-nbsp = 
    ## Whether to justify NON-BREAKING SPACE
    attribute axf:justify-nbsp { 'true' | 'false' }?
axf_kansuji-grouping-letter = 
    ## Grouping character used for Japanese numerals
    attribute axf:kansuji-grouping-letter { text }?
axf_kansuji-letter = 
    ## Character used for Japanese numerals: kanji | latin | <string>
    attribute axf:kansuji-letter { text }?
axf_kansuji-style = 
    ## Style used for Japanese numerals
    attribute axf:kansuji-style { 'simple' | 'grouping' | 'readable' }?
axf_kerning-mode = 
    ## Whether to perform kerning
    attribute axf:kerning-mode { 'none' | 'pair' | 'auto' }?
axf_layer-name = 
    ## Layer to which the area is arranged.
    attribute axf:layer-name { 'none' | text }?
axf_leader-expansion = 
    ## Whether to forcibly expand a leader
    attribute axf:leader-expansion { 'auto' | 'force' }?
axf_letter-spacing-side = 
    ## Which side of the character the space by letter-spacing is distributed
    attribute axf:letter-spacing-side { 'both' | 'start' | 'end' }?
axf_ligature-mode = 
    ## Whether to perform the ligature processing: none | [ latin || kana ] | all | auto
    attribute axf:ligature-mode { text }?
axf_line-break = 
    ## Line-breaking method
    attribute axf:line-break { 'normal' | 'strict' }?
axf_line-continued-mark = 
    ## Whether to show line continued marks: <string>
    attribute axf:line-continued-mark { text }?
axf_line-continued-mark-background-color = 
    ## Background color of line continued marks
    attribute axf:line-continued-mark-background-color { text }?
axf_line-continued-mark-color = 
    ## Color of line continued marks
    attribute axf:line-continued-mark-color { text }?
axf_line-continued-mark-font-family = 
    ## Font family of line continued marks
    attribute axf:line-continued-mark-font-family { text }?
axf_line-continued-mark-font-size = 
    ## Font size of line continued marks
    attribute axf:line-continued-mark-font-size { text }?
axf_line-continued-mark-font-style = 
    ## Whether to make the font style italic
    attribute axf:line-continued-mark-font-style { 'normal' | 'italic' }?
axf_line-continued-mark-font-weight = 
    ## Font weight of line numbers
    attribute axf:line-continued-mark-font-weight { 'normal' | 'bold' | 'bolder' | 'lighter' | '100' | '200' | '300' | '400' | '500' | '600' | '700' | '800' | '900' }?
axf_line-continued-mark-offset = 
    ## Offset of line continued marks: <length>
    attribute axf:line-continued-mark-offset { text }?
axf_line-number = 
    ## Whether to show line numbers
    attribute axf:line-number { 'none' | 'show' | 'hide' }?
axf_line-number-background-color = 
    ## Background color of line numbers
    attribute axf:line-number-background-color { text }?
axf_line-number-color = 
    ## Color of line numbers
    attribute axf:line-number-color { text }?
axf_line-number-display-align = 
    ## Alignment, in the block-progression-direction, of line numbers in the line area
    attribute axf:line-number-display-align { 'auto' | 'before' | 'center' | 'after' }?
axf_line-number-font-family = 
    ## Font family of line numbers
    attribute axf:line-number-font-family { text }?
axf_line-number-font-size = 
    ## Font size of line numbers
    attribute axf:line-number-font-size { text }?
axf_line-number-font-style = 
    ## Whether to make the font style italic
    attribute axf:line-number-font-style { 'normal' | 'italic' }?
axf_line-number-font-weight = 
    ## Font weight of line numbers
    attribute axf:line-number-font-weight { 'normal' | 'bold' | 'bolder' | 'lighter' | '100' | '200' | '300' | '400' | '500' | '600' | '700' | '800' | '900' }?
axf_line-number-offset = 
    ## Offset of line numbers: <length>
    attribute axf:line-number-offset { text }?
axf_line-number-position = 
    ## Position of line numbers
    attribute axf:line-number-position { 'start' | 'end' | 'inside' | 'outside' | 'alternate' }?
axf_line-number-text-align = 
    ## Alignment of line numbers in the line area
    attribute axf:line-number-text-align { 'auto' | 'start' | 'center' | 'end' | 'inside' | 'outside' | 'left' | 'right' }?
axf_line-number-text-decoration = 
    ## Text decoration of line numbers
    attribute axf:line-number-text-decoration { text }?
axf_line-number-text-width = 
    ## Width of line numbers: auto | <length>
    attribute axf:line-number-text-width { text }?
axf_list-style-type = 
    ## List style: <glyph> | <algorithmic> | <numeric> | <alphabetic> | <symbolic> | <non-repeating> | normal | none
    attribute axf:list-style-type { text }?
axf_normalize = 
    ## Unicode normalization to perform on text
    attribute axf:normalize { 'auto' | 'none' | 'nfc' | 'nfkc' | 'nfd' | 'nfkd' }?
axf_normalize-exclude = 
    ## Whether Composition Exclusions are excluded or not when the normalization (axf:normalize) is specified
    attribute axf:normalize-exclude { 'full-composition-exclusion' | 'none' }?
axf_number-transform = 
    ## Converts the number sequence in the character string: none | kansuji | kansuji-if-vertical | <list-style-type> | <string>
    attribute axf:number-transform { text }?
axf_overflow-align = 
    ## Alignment of the overflowed block
    attribute axf:overflow-align { 'normal' | 'start' | 'end' | 'center' }?
axf_overflow-condense = 
    ## How to condense the overflowed text within the region
    attribute axf:overflow-condense { 'letter-spacing' | 'font-stretch' | 'font-size' | 'line-height' | 'auto' | 'none' }?
axf_overflow-condense-limit-font-size = 
    ## Minimum font size when axf:overflow-condense="font-size" is specified: <length> [ visible | hidden | scroll | error-if-overflow | repeat ]
    attribute axf:overflow-condense-limit-font-size { text }?
axf_overflow-condense-limit-font-stretch = 
    ## Minimum value when axf:overflow-condense="font-stretch" is specified: [ <number> | <percentage> ] [ visible | hidden | scroll | error-if-overflow | repeat ]
    attribute axf:overflow-condense-limit-font-stretch { text }?
axf_overflow-limit = 
    ## Overflow limit value: <length>1 2
    attribute axf:overflow-limit { text }?
axf_overflow-limit-block = 
    ## Block overflow limit value
    attribute axf:overflow-limit-block { text }?
axf_overflow-limit-inline = 
    ## Inline overflow limit value
    attribute axf:overflow-limit-inline { text }?
axf_overflow-replace = 
    ## Alternative character string for the specified overflow text
    attribute axf:overflow-replace { text }?
axf_overprint = 
    ## Overprint setting
    attribute axf:overprint { text }?
axf_punctuation-spacing = 
    ## Space to trim  between a full width punctuation and a full width character in Japanese
    attribute axf:punctuation-spacing { text }?
axf_punctuation-trim = 
    ## How to treat full width punctuations in Japanese
    attribute axf:punctuation-trim { text }?
axf_repeat-footnote-in-table-footer = 
    ## Whether to repeat the fo:footnote in the fo:table-footer that is repeated by table-omit-footer-at-break="false"
    attribute axf:repeat-footnote-in-table-footer { 'true' | 'false' }?
axf_repeat-footnote-in-table-header = 
    ## Whether to repeat the fo:footnote in the fo:table-header that is repeated by table-omit-header-at-break="false"
    attribute axf:repeat-footnote-in-table-header { 'true' | 'false' }?
axf_reverse-diagonal-border-color = 
    ## Color of the reverse diagonal border
    attribute axf:reverse-diagonal-border-color { text }?
axf_reverse-diagonal-border-width = 
    ## Width of the reverse diagonal border
    attribute axf:reverse-diagonal-border-width { text }?
axf_revision-bar-color = 
    ## Color of the revision bar
    attribute axf:revision-bar-color { text }?
axf_revision-bar-offset = 
    ## Offset of the revision bar
    attribute axf:revision-bar-offset { text }?
axf_revision-bar-position = 
    ## Position of the revision bar
    attribute axf:revision-bar-position { 'start' | 'end' | 'inside' | 'outside' | 'alternate' | 'both' }?
axf_revision-bar-style = 
    ## Style of the revision bar
    attribute axf:revision-bar-style { text }?
axf_revision-bar-width = 
    ## Width of the revision bar
    attribute axf:revision-bar-width { text }?
axf_soft-hyphen-treatment = 
    ## Whether to print SOFT HYPHEN (U+00AD)
    attribute axf:soft-hyphen-treatment { 'auto' | 'preserve' }?
axf_suppress-duplicate-footnote = 
    ## Whether to delete footnotes duplicated in the same page
    attribute axf:suppress-duplicate-footnote { 'true' | 'false' }?
axf_suppress-duplicate-page-number = 
    ## Whether to delete duplicated page numbers
    attribute axf:suppress-duplicate-page-number { 'true' | 'false' }?
axf_text-align-first = 
    ## Text alignment of the first line
    attribute axf:text-align-first { 'relative' | 'start' | 'center' | 'end' | 'justify' | 'inside' | 'outside' | 'left' | 'right' }?
axf_text-align-string = 
    ## Text alignment when text-align="<string>"
    attribute axf:text-align-string { 'start' | 'center' | 'end' | 'inside' | 'outside' | 'left' | 'right' }?
axf_text-autospace = 
    ## Whether to add space surrounding ideographic glyphs: none | [ ideograph-numeric || ideograph-alpha || ideograph-parenthesis ] | auto
    attribute axf:text-autospace { text }?
axf_text-autospace-width = 
    ## Width for axf:text-autospace
    attribute axf:text-autospace-width { text }?
axf_text-combine-horizontal = 
    ## Sets horizontal-in-vertical composition in vertical writing mode automatically: none | all | [ digits <integer> || alpha <integer> || alphanumeric <integer> ]
    attribute axf:text-combine-horizontal { text }?
axf_text-emphasis-font-color = 
    ## Color of emphasis marks
    attribute axf:text-emphasis-font-color { text }?
axf_text-emphasis-font-family = 
    ## Font family of emphasis marks
    attribute axf:text-emphasis-font-family { text }?
axf_text-emphasis-font-size = 
    ## Font size of emphasis marks
    attribute axf:text-emphasis-font-size { text }?
axf_text-emphasis-font-stretch = 
    ## Font stretching of emphasis marks
    attribute axf:text-emphasis-font-stretch { text }?
axf_text-emphasis-font-style = 
    ## Whether emphasis marks are made Italic
    attribute axf:text-emphasis-font-style { 'normal' | 'italic' }?
axf_text-emphasis-font-weight = 
    ## Font weight of emphasis marks
    attribute axf:text-emphasis-font-weight { 'normal' | 'bold' | 'bolder' | 'lighter' | '100' | '200' | '300' | '400' | '500' | '600' | '700' | '800' | '900' }?
axf_text-emphasis-offset = 
    ## Space between emphasis marks and the base characters
    attribute axf:text-emphasis-offset { text }?
axf_text-emphasis-position = 
    ## Which side of base characters emphasis marks are put
    attribute axf:text-emphasis-position { 'before' | 'after' }?
axf_text-emphasis-skip = 
    ## Characters to which emphasis marks are not applied: none | [ spaces || punctuation || symbols || narrow ]
    attribute axf:text-emphasis-skip { text }?
axf_text-emphasis-style = 
    ## Style of emphasis marks: none | [ [ filled | open ] || [ dot | circle | double-circle | triangle | sesame ] ] | <string> 
    attribute axf:text-emphasis-style { text }?
axf_text-justify-trim = 
    ## The way to trim spaces between characters in justified text
    attribute axf:text-justify-trim { text }?
axf_text-kashida-space = 
    ## Percentage of Kashida in Arabic justification: <percentage> | auto
    attribute axf:text-kashida-space { text }?
axf_text-orientation = 
    ## Orientation of text in vertical writing mode
    attribute axf:text-orientation { 'mixed' | 'upright' | 'sideways-right' | 'sideways' | 'none' }?
axf_vertical-underline-side = 
    ## Which side of the text to put underline in vertical writing-mode
    attribute axf:vertical-underline-side { 'left' | 'right' | 'depend-on-language' | 'auto' }?
axf_word-break = 
    ## Whether to enable line breaking even inside a word
    attribute axf:word-break { 'normal' | 'break-all' | 'keep-all' }?
axf_word-wrap = 
    ## Whether to forcibly break a word when line break cannot be performed
    attribute axf:word-wrap { 'normal' | 'break-word' }?
